                    const cancelNcDetailsBtn = document.getElementById('cancelNcDetailsBtn');
                    let currentJobForNCSubmission = null; // To hold the job object for NC submission
                    let currentNcToEdit = null; // To hold the NC object being edited

                    // New NC Completion Fields
