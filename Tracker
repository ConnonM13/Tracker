            <script>
                // JavaScript for the job tracker logic
                document.addEventListener('DOMContentLoaded', () => {
                    // Main page elements
                    const showAddJobModalBtn = document.getElementById('showAddJobModalBtn');

                    const showActiveJobsBtn = document.getElementById('showActiveJobsBtn');
                    const showCompletedJobsBtn = document.getElementById('showCompletedJobsBtn');
                    const showAllJobsBtn = document.getElementById('showAllJobsBtn');
                    const showManageProcessesBtn = document.getElementById('showManageProcessesBtn');

                    const activeDashboard = document.getElementById('activeDashboard');
                    const completedJobsSection = document.getElementById('completedJobsSection');
                    const allJobsMasterListSection = document.getElementById('allJobsMasterListSection');
                    const manageProcessesSection = document.getElementById('manageProcessesSection');

                    const processSelectNav = document.getElementById('processSelectNav');
                    const currentProcessQueueCard = document.getElementById('currentProcessQueueCard');
                    const currentProcessQueueTitle = document.getElementById('currentProcessQueueTitle');
                    const currentProcessQueueList = document.getElementById('currentProcessQueueList');

                    const completedJobsList = document.getElementById('completedJobsList');
                    const allJobsList = document.getElementById('allJobsList');
                    const manageProcessesList = document.getElementById('manageProcessesList');
                    const createTestJobButton = document.getElementById('create-test-job-button'); // New test job button

                    const messageBox = document.getElementById('messageBox');

                    // Print Buttons
                    const printCurrentProcessJobsBtn = document.getElementById('printCurrentProcessJobsBtn');
                    const printAllJobsBtn = document.getElementById('printAllJobsBtn');
                    const printShippingListBtn = document.getElementById('printShippingListBtn');

                    // Alert Banner elements
                    const warningAlertBanner = document.getElementById('warningAlertBanner');
                    const alertTextSpanWarning = document.getElementById('alertTextSpanWarning');

                    const errorAlertBanner = document.getElementById('errorAlertBanner'); // This will now be hidden
                    const alertTextSpanError = document.getElementById('alertTextSpanError'); // This will now be hidden

                    const manageAlertsBtn = document.getElementById('manageAlertsBtn');
                    const manageNcBtn = document.getElementById('manageNcBtn'); // New Manage NCs button
                    

                    // Add/Edit Job Modal elements
                    const addJobModal = document.getElementById('addJobModal');
                    const addJobModalTitle = document.getElementById('addJobModalTitle');
                    const addJobModalValidationMessage = document.getElementById('addJobModalValidationMessage'); // New validation message element
                    const modalJobDescriptionInput = document.getElementById('modalJobDescription');
                    const modalJobNumberInput = document.getElementById('modalJobNumber');
                    const modalCustomerNameInput = document.getElementById('modalCustomerName');
                    const modalDrawingNumberInput = document.getElementById('modalDrawingNumber');
                    const modalDueDateInput = document.getElementById('modalDueDate');
                    const modalPriorityInput = document.getElementById('modalPriority');
                    const modalIsASMESelect = document.getElementById('modalIsASME');
                    const modalQuantityInput = document.getElementById('modalQuantity');
                    const modalAddJobBtn = document.getElementById('modalAddJobBtn');
                    const modalCancelAddJobBtn = document.getElementById('modalCancelAddJobBtn');
                    const modalIsRework = document.getElementById('modalIsRework'); // New rework checkbox

                    // New Folder Locations
                    const modalJobFilesFolderLocation = document.getElementById('modalJobFilesFolderLocation');
                    const modalDxfFilesFolderLocation = document.getElementById('modalDxfFilesFolderLocation');
