                    // Removed completeNcBtn as it's no longer needed

                    // New View NC Details Modal elements
                    const viewNcDetailsModal = document.getElementById('viewNcDetailsModal');
                    const ncJobDescriptionDisplay = document.getElementById('ncJobDescriptionDisplay');
                    const ncDetailsList = document.getElementById('ncDetailsList');
                    const ncDetailsPlaceholder = document.getElementById('ncDetailsPlaceholder');
                    const closeNcDetailsModalBtn = document.getElementById('closeNcDetailsModalBtn');
                    let currentJobForNCView = null; // Used for job-specific NC view
                    // Removed currentSingleNcToView as the "Details" button is gone

                    // New Manage All NCs Modal elements
                    const manageAllNcModal = document.getElementById('manageAllNcModal');
                    const ncSearchInput = document.getElementById('ncSearchInput');
                    const ncSearchBtn = document.getElementById('ncSearchBtn');
                    const downloadNcListBtn = document.getElementById('downloadNcListBtn');
                    const allNcList = document.getElementById('allNcList');
                    const closeManageAllNcModalBtn = document.getElementById('closeManageAllNcModalBtn');
                    const ncFilterCaughtInSelect = document.getElementById('ncFilterCaughtInSelect'); // New filter
                    const ncFilterCausedBySelect = document.getElementById('ncFilterCausedBySelect'); // New filter
                    const ncFilterStatusSelect = document.getElementById('ncFilterStatusSelect'); // New status filter
                    const clearNcFiltersBtn = document.getElementById('clearNcFiltersBtn'); // New button

                    // Add/Edit Process Modal elements
                    const addEditProcessModal = document.getElementById('addEditProcessModal');
                    const addEditProcessModalTitle = document.getElementById('addEditProcessModalTitle');
                    const addEditProcessModalValidationMessage = document.getElementById('addEditProcessModalValidationMessage'); // New validation message element
                    const processNameInput = document.getElementById('processNameInput');
                    const processDefaultAssignedToInput = document.getElementById('processDefaultAssignedToInput');
                    const processDefaultIsOutsideProcessSelect = document.getElementById('processDefaultIsOutsideProcessSelect');
                    const processDefaultOutsideProviderInput = document.getElementById('processDefaultOutsideProviderInput');
                    const processDefaultOutsideProviderRow = document.getElementById('processDefaultOutsideProviderRow');
                    const saveProcessDetailsBtn = document.getElementById('saveProcessDetailsBtn');
                    const cancelProcessDetailsBtn = document.getElementById('cancelProcessDetailsBtn');
                    const showAddProcessModalBtn = document.getElementById('showAddProcessModalBtn');

                    // Add/Edit Alert Modal elements
                    const addEditAlertModal = document.getElementById('addEditAlertModal');
                    const addEditAlertModalTitle = document.getElementById('addEditAlertModalTitle');
                    const addEditAlertModalValidationMessage = document.getElementById('addEditAlertModalValidationMessage'); // New validation message element
                    const newAlertMessageInput = document.getElementById('newAlertMessageInput');
                    const addUpdateAlertBtn = document.getElementById('addUpdateAlertBtn'); // Renamed button
                    const clearAllAlertsBtn = document.getElementById('clearAllAlertsBtn');
                    const cancelAlertModalBtn = document.getElementById('cancelAlertModalBtn');
                    const activeAlertsList = document.getElementById('activeAlertsList'); // New: List for alerts
                    let currentAlertIndex = null; // New: To track which alert is being edited

                    // Job-specific Shop Notes Modal elements
                    const shopNotesModal = document.getElementById('shopNotesModal');
                    const shopNotesModalTitle = document.getElementById('shopNotesModalTitle');
                    const shopNotesInput = document.getElementById('shopNotesInput');
                    const saveShopNotesBtn = document.getElementById('saveShopNotesBtn');
                    const cancelShopNotesBtn = document.getElementById('cancelShopNotesBtn');


                    let jobs = [];
                    let processOptions = [];
                    let storedAlerts = [];
                    let currentJobToEdit = null;
                    let currentJobBeingEdited = null;
                    let currentProcessToEdit = null;
                    let currentJobForNotes = null;
                    let currentMainView = 'active';
                    let currentProcessView = '';
                    let isAsmeSorted = false; // New state variable for ASME sorting

                    const defaultProcessOptions = [
                        { name: "Design", defaultAssignedTo: "Engineering", defaultIsOutsideProcess: false, defaultOutsideProvider: "" },
                        { name: "Programming", defaultAssignedTo: "CNC Department", defaultIsOutsideProcess: false, defaultOutsideProvider: "" },
                        { name: "Review", defaultAssignedTo: "Quality Control", defaultIsOutsideProcess: false, defaultOutsideProvider: "" },
                        { name: "Laser/Plasma", defaultAssignedTo: "Cutting Dept.", defaultIsOutsideProcess: false, defaultOutsideProvider: "" },
                        { name: "Saw", defaultAssignedTo: "Cutting Dept.", defaultIsOutsideProcess: false, defaultOutsideProvider: "" },
                        { name: "Press", defaultAssignedTo: "Fabrication", defaultIsOutsideProcess: false, defaultOutsideProvider: "" },
                        { name: "Welding", defaultAssignedTo: "Welding Dept.", defaultIsOutsideProcess: false, defaultOutsideProvider: "" },
                        { name: "Paint", defaultAssignedTo: "Paint Shop", defaultIsOutsideProcess: true, defaultOutsideProvider: "Local Powder Coaters" },
                        { name: "Inspection", defaultAssignedTo: "Quality Control", defaultIsOutsideProcess: false, defaultOutsideProvider: "" },
                        { name: "Shipping", defaultAssignedTo: "Shipping Dept.", defaultIsOutsideProcess: false, defaultOutsideProvider: "" }
                    ];

                    // --- Utility Functions ---

                    const showMessage = (message, type = 'success') => {
                        messageBox.textContent = message;
                        messageBox.className = `message-box show ${type}`;
                        setTimeout(() => {
                            messageBox.classList.remove('show');
                        }, 3000);
                    };

                    // New: Function to display modal-specific validation messages
                    const showModalValidationMessage = (modalMessageElement, message) => {
                        modalMessageElement.textContent = message;
                        modalMessageElement.style.display = 'block';
                        modalMessageElement.classList.add('show');
                    };

                    const hideModalValidationMessage = (modalMessageElement) => {
                        modalMessageElement.style.display = 'none';
                        modalMessageElement.classList.remove('show');
                    };

                    // New: Function to highlight required fields
                    const validateAndHighlight = (elements, messageElement) => {
                        let allValid = true;
                        elements.forEach(el => {
                            // Handle specific cases for validation
                            if (el.id === 'requiredProcessesList') {
                                // For the required processes list, check if tempRequiredProcesses has items
                                if (tempRequiredProcesses.length === 0) {
                                    allValid = false;
                                    // No direct highlight, but the section itself might be visually indicated if desired
                                    // For now, just mark invalid and show message
                                }
                            } else if (el.value.trim() === '' || (el.tagName === 'SELECT' && el.value === '')) {
                                el.classList.add('input-error');
                                allValid = false;
                            } else {
                                el.classList.remove('input-error');
                            }
                        });

                        if (!allValid) {
                            showModalValidationMessage(messageElement, 'Please populate all required fields.');
                        } else {
                            hideModalValidationMessage(messageElement);
                        }
                        return allValid;
                    };

                    // New: Function to clear all input-error classes in a given modal
                    const clearValidationHighlights = (modalElement) => {
                        modalElement.querySelectorAll('.input-error').forEach(el => {
                            el.classList.remove('input-error');
                        });
                        // Also hide any validation messages
                        modalElement.querySelectorAll('.message-box.error').forEach(msgBox => {
                            hideModalValidationMessage(msgBox);
                        });
                    };

                    // New: Add event listeners to clear error on input
                    const addInputErrorClearListeners = (modalElement) => {
                        modalElement.querySelectorAll('input, select, textarea').forEach(el => {
                            el.addEventListener('input', () => {
                                el.classList.remove('input-error');
                                // Optionally, if all fields become valid, hide the modal message
                                // This might be too aggressive, so keeping it simple for now.
                            });
                            el.addEventListener('change', () => { // For selects
                                el.classList.remove('input-error');
                            });
                        });
                    };


                    const renderAlertBanners = () => {
                        // Combine all alert messages into a single string for the warning banner
                        const allAlertMessages = storedAlerts.map(alert => alert.message);

                        if (allAlertMessages.length > 0) {
                            const combinedMessage = allAlertMessages.join(' ----- ');
                            alertTextSpanWarning.textContent = combinedMessage;
                            warningAlertBanner.style.display = 'block';

                            const textWidth = alertTextSpanWarning.scrollWidth;
                            const containerWidth = warningAlertBanner.clientWidth;

                            if (textWidth > containerWidth || allAlertMessages.length > 1) {
                                alertTextSpanWarning.parentElement.classList.remove('no-scroll');
                                const duration = Math.max(20, textWidth * 0.1);
                                alertTextSpanWarning.parentElement.style.animation = `marquee ${duration}s linear infinite`;
                            } else {
                                alertTextSpanWarning.parentElement.classList.add('no-scroll');
                                alertTextSpanWarning.parentElement.style.animation = 'none';
                            }
                        } else {
                            warningAlertBanner.style.display = 'none';
                            alertTextSpanWarning.textContent = '';
                            alertTextSpanWarning.parentElement.style.animation = 'none';
                        }

                        // Ensure the error banner is always hidden
                        errorAlertBanner.style.display = 'none';
                        alertTextSpanError.textContent = '';
                        alertTextSpanError.parentElement.style.animation = 'none';
                    };


                    const loadData = () => {
                        const savedJobs = localStorage.getItem('jobTrackerJobs');
                        if (savedJobs) {
                            jobs = JSON.parse(savedJobs);
                            // Ensure new properties are initialized for existing jobs
                            jobs.forEach(job => {
                                if (job.nonConformances === undefined) {
                                    job.nonConformances = [];
                                }
                                if (job.reworkQuantity === undefined) {
                                    job.reworkQuantity = 0;
                                }
                                if (job.isRework === undefined) { // Initialize new field
                                    job.isRework = false;
                                }
                                if (job.originalJobId === undefined) { // Initialize new field
                                    job.originalJobId = null;
                                }
                                if (job.jobFilesFolderLocation === undefined) { // New field
                                    job.jobFilesFolderLocation = '';
                                }
                                if (job.dxfFilesFolderLocation === undefined) { // New field
                                    job.dxfFilesFolderLocation = '';
                                }
                                // Ensure NCs have IDs and new completion properties
                                job.nonConformances.forEach(nc => {
                                    if (nc.id === undefined) {
                                        nc.id = Date.now() + Math.floor(Math.random() * 1000); // Assign a unique ID if missing
                                    }
                                    if (nc.otherActionNotes === undefined) { // Initialize new field
                                        nc.otherActionNotes = '';
                                    }
                                    if (nc.isClosed === undefined) { // New field
                                        nc.isClosed = false;
                                    }
                                    if (nc.completionDate === undefined) { // New field
                                        nc.completionDate = null;
                                    }
                                    if (nc.completedBy === undefined) { // New field
                                        nc.completedBy = '';
                                    }
                                    if (nc.completionAction === undefined) { // New field
                                        nc.completionAction = '';
                                    }
                                });
                            });
                        }
                        const savedProcesses = localStorage.getItem('jobTrackerProcesses');
                        if (savedProcesses) {
                            processOptions = JSON.parse(savedProcesses);
                        } else {
                            processOptions = defaultProcessOptions;
                        }
                        const savedAlerts = localStorage.getItem('jobTrackerAlerts');
                        if (savedAlerts) {
                            storedAlerts = JSON.parse(savedAlerts);
                        } else {
                            storedAlerts = [];
                        }
                        renderAlertBanners();

                        if (processOptions.length > 0 && !processOptions.some(p => p.name === currentProcessView)) {
                            currentProcessView = processOptions[0].name;
                        } else if (processOptions.length === 0) {
                            currentProcessView = '';
                        }
                        populateProcessDropdowns();
                        populateNcFilterDropdowns(); // Populate NC filter dropdowns on load
                        renderJobs();
                        renderProcessList();
                    };

                    const saveData = () => {
                        localStorage.setItem('jobTrackerJobs', JSON.stringify(jobs));
                        localStorage.setItem('jobTrackerProcesses', JSON.stringify(processOptions));
                        localStorage.setItem('jobTrackerAlerts', JSON.stringify(storedAlerts));
                    };

                    const populateProcessDropdowns = () => {
                        availableProcessesSelect.innerHTML = '';
                        processSelectNav.innerHTML = '';
                        ncProcessCaughtSelect.innerHTML = ''; // For NC modal
                        ncProcessCausedSelect.innerHTML = ''; // For NC modal

                        // Add a default "Select Process" option for NC dropdowns
                        const defaultNcOption = document.createElement('option');
                        defaultNcOption.value = '';
                        defaultNcOption.textContent = '-- Select Process --';
                        defaultNcOption.disabled = true;
                        defaultNcOption.selected = true;
                        ncProcessCaughtSelect.appendChild(defaultNcOption.cloneNode(true));
                        ncProcessCausedSelect.appendChild(defaultNcOption.cloneNode(true));


                        processOptions.forEach(process => {
                            const opt1 = document.createElement('option');
                            opt1.value = process.name;
                            opt1.textContent = process.name;
                            availableProcessesSelect.appendChild(opt1);

                            const opt2 = document.createElement('option');
                            opt2.value = process.name;
                            opt2.textContent = process.name;
                            processSelectNav.appendChild(opt2);

                            const opt3 = document.createElement('option'); // For NC Caught
                            opt3.value = process.name;
                            opt3.textContent = process.name;
                            ncProcessCaughtSelect.appendChild(opt3);

                            const opt4 = document.createElement('option'); // For NC Caused
                            opt4.value = process.name;
                            opt4.textContent = process.name;
                            ncProcessCausedSelect.appendChild(opt4);
                        });
                    };

                    const populateNcFilterDropdowns = () => {
                        ncFilterCaughtInSelect.innerHTML = '<option value="">Filter by Caught In</option>';
                        ncFilterCausedBySelect.innerHTML = '<option value="">Filter by Caused By</option>';

                        processOptions.forEach(process => {
                            const optCaught = document.createElement('option');
                            optCaught.value = process.name;
                            optCaught.textContent = process.name;
                            ncFilterCaughtInSelect.appendChild(optCaught);

                            const optCaused = document.createElement('option');
                            optCaused.value = process.name;
                            optCaused.textContent = process.name;
                            ncFilterCausedBySelect.appendChild(optCaused);
                        });
                    };

                    // --- Job Rendering ---

                    const renderJobs = () => {
                        currentProcessQueueList.innerHTML = '';
                        completedJobsList.innerHTML = '';
                        allJobsList.innerHTML = '';

                        const activeJobs = jobs.filter(job => !job.completed);
                        const completedJobs = jobs.filter(job => job.completed);

                        // Determine if there are any ASME jobs
                        const hasAsmeJobs = jobs.some(job => job.isASME);

                        // Sorting function for active jobs
                        const sortJobs = (a, b) => {
                            // Primary sort: ASME jobs first if isAsmeSorted is true AND there are ASME jobs
                            if (isAsmeSorted && hasAsmeJobs) {
                                if (a.isASME && !b.isASME) return -1;
                                if (!a.isASME && b.isASME) return 1;
                            }

                            // Secondary sort: Due Date
                            const dateA = a.dueDate ? new Date(a.dueDate) : new Date('9999-12-31');
                            const dateB = b.dueDate ? new Date(b.dueDate) : new Date('9999-12-31');
                            if (dateA.getTime() !== dateB.getTime()) {
                                return dateA - dateB;
                            }

                            // Tertiary sort: Priority
                            const priorityOrder = { 'high': 1, 'medium': 2, 'low': 3 };
                            return priorityOrder[a.priority] - priorityOrder[b.priority];
                        };

                        const sortedActiveJobs = [...activeJobs].sort(sortJobs);

                        const sortedCompletedJobs = [...completedJobs].sort((a, b) => {
                            const dateA = a.completionDate ? new Date(a.completionDate) : new Date(a.addedDate);
                            const dateB = b.completionDate ? new Date(b.completionDate) : new Date(b.addedDate);
                            return dateB - dateA;
                        });

                        if (currentMainView === 'active') {
                            const jobsInCurrentProcess = sortedActiveJobs.filter(job => {
                                const currentProcessName = job.requiredProcesses[job.currentProcessStep];
                                return currentProcessName === currentProcessView;
                            });
                            currentProcessQueueTitle.textContent = currentProcessView || 'No Processes Defined';
                            if (jobsInCurrentProcess.length > 0) {
                                jobsInCurrentProcess.forEach(job => {
                                    currentProcessQueueList.appendChild(createJobElement(job));
                                });
                            } else {
                                currentProcessQueueList.innerHTML = '<p class="placeholder-text">No jobs in this stage.</p>';
                            }
                        } else if (currentMainView === 'completed') {
                            if (sortedCompletedJobs.length > 0) {
                                sortedCompletedJobs.forEach(job => {
                                    completedJobsList.appendChild(createJobElement(job, false, true));
                                });
                            } else {
                                completedJobsList.innerHTML = '<p class="placeholder-text">No completed jobs yet.</p>';
                            }
                        } else if (currentMainView === 'allJobs') {
                            const allSortedJobs = [...jobs].sort((a, b) => {
                                // First, separate completed from active
                                if (a.completed !== b.completed) {
                                    return a.completed ? 1 : -1; // Completed jobs come after active jobs
                                }

                                // Then, apply the common sorting logic (including ASME if enabled)
                                return sortJobs(a, b);
                            });

                            if (allSortedJobs.length > 0) {
                                allSortedJobs.forEach(job => {
                                    allJobsList.appendChild(createJobElement(job, false, job.completed));
                                });
                            } else {
                                allJobsList.innerHTML = '<p class="placeholder-text">No jobs to display.</p>';
                            }
                        }

                        saveData();
                    };

                    const createJobElement = (job, isCompact = false, isCompletedList = false) => {
                        const li = document.createElement('li');
                        li.dataset.id = job.id;
                        li.classList.add('job-item');

                        const now = new Date();
                        const dueDate = job.dueDate ? new Date(job.dueDate) : null;
                        const isLate = dueDate && !job.completed && dueDate < now;
                        const isHighPriority = job.priority === 'high';
                        const isASME = job.isASME;

                        const currentProcessName = job.requiredProcesses[job.currentProcessStep];
                        const currentProcessObj = processOptions.find(p => p.name === currentProcessName);
                        const assignedTo = currentProcessObj ? currentProcessObj.defaultAssignedTo : 'N/A';
                        const isOutsideProcess = currentProcessObj ? currentProcessObj.defaultIsOutsideProcess : false;
                        const outsideProvider = currentProcessObj ? currentProcessObj.defaultOutsideProvider : 'N/A';

                        const pdfDisplayText = job.drawingPdfBase64
                            ? (job.drawingPdfFileName ? `PDF Drawing: ${job.drawingPdfFileName}` : 'PDF Drawing: Attached')
                            : '';


                        if (isLate) {
                            li.classList.add('late');
                        }
                        if (isHighPriority) {
                            li.classList.add('high-priority');
                        }
                        if (job.completed) {
                            li.classList.add('completed');
                            if (isCompletedList) {
                                li.classList.add('completed-list-item');
                            }
                        }
                        if (job.nonConformances && job.nonConformances.length > 0) {
                            li.classList.add('has-nc');
                        }

                        if (isCompact) {
                            li.innerHTML = `
                                                    <span class="job-name">
                                                        ${job.jobNumber ? `<span class="job-number">#${job.jobNumber}</span>` : ''}
                                                        <span class="job-main-name">${job.description}</span>
                                                        ${job.customer ? `<span class="job-customer">(${job.customer})</span>` : ''}
                                                    </span>
                                                    <span class="job-details">${dueDate ? new Date(job.dueDate).toLocaleDateString() : 'No Due Date'}</span>
                                                `;
                        } else {
                            const isLastProcessStep = (job.currentProcessStep === job.requiredProcesses.length - 1);
                            const completeButtonText = isLastProcessStep ? 'Complete Job' : 'Next Stage';
                            const completeButtonIcon = isLastProcessStep ? 'fas fa-check-double' : 'fas fa-arrow-right';

                            li.innerHTML = `
                                                    <div class="job-header">
                                                        <span class="job-identifier">
                                                            ${job.jobNumber ? `<span class="job-number">#${job.jobNumber}</span>` : ''}
                                                            <span class="job-main-name">${job.description}</span>
                                                            ${job.customer ? `<span class="job-customer">(${job.customer})</span>` : ''}
                                                            ${job.isRework ? `<span class="job-rework-tag" style="background-color: #f0ad4e; color: white; padding: 2px 6px; border-radius: 4px; font-size: 0.7em; margin-left: 8px;">REWORK</span>` : ''}
                                                        </span>
                                                        <span class="job-priority">Priority: ${job.priority.charAt(0).toUpperCase() + job.priority.slice(1)}</span>
                                                    </div>
                                                    <div class="job-actions">
                                                        ${!job.completed ? `<button class="complete-btn"><i class="${completeButtonIcon}"></i> ${completeButtonText}</button>` : `<button class="complete-btn"><i class="fas fa-undo"></i> Undo Completion</button>`}
                                                        ${!job.completed ? `<button class="edit-status-btn" data-job-id="${job.id}"><i class="fas fa-edit"></i> Manage Workflow</button>` : ''}
                                                        ${!job.completed ? `<button class="edit-job-btn" data-job-id="${job.id}"><i class="fas fa-pencil-alt"></i> Edit Job</button>` : ''}
                                                        <button class="add-shop-notes-btn" data-job-id="${job.id}"><i class="fas fa-clipboard"></i> Shop Notes</button>
                                                        ${job.drawingPdfBase64 || job.eDrawingReference || job.drawingNumber ? `<button class="view-drawing-btn" data-job-id="${job.id}"><i class="fas fa-file-pdf"></i> View Drawing</button>` : ''}
                                                        ${job.nonConformances && job.nonConformances.length > 0 ? `<button class="view-nc-details-btn" data-job-id="${job.id}"><i class="fas fa-exclamation-triangle"></i> View NCs (${job.nonConformances.length})</button>` : ''}
                                                        <button class="details-toggle-btn"><i class="fas fa-info-circle"></i> Show Details</button>
                                                        <button class="delete-btn"><i class="fas fa-trash-alt"></i> Delete</button>
                                                    </div>
                                                    <div class="job-expandable-content">
                                                        <div class="job-details-grid-container">
                                                            ${job.drawingNumber ? `<span><strong>Drawing No:</strong> ${job.drawingNumber}</span>` : ''}
                                                            ${pdfDisplayText ? `<span><strong>${pdfDisplayText.split(':')[0]}:</strong> ${pdfDisplayText.split(':')[1] || 'Attached'}</span>` : ''}
                                                            ${job.quantity ? `<span><strong>Quantity:</strong> ${job.quantity}</span>` : ''}
                                                            ${job.reworkQuantity !== undefined && job.reworkQuantity !== null && job.reworkQuantity > 0 ? `<span><strong>Rework Quantity:</strong> ${job.reworkQuantity}</span>` : ''}
                                                            ${dueDate ? `<span><strong>Due Date:</strong> ${new Date(job.dueDate).toLocaleDateString()}</span>` : '<span><strong>Due Date:</strong> No Due Date</span>'}
                                                            <span><strong>Added:</strong> ${new Date(job.addedDate).toLocaleDateString()}</span>
                                                            ${job.completed ? `<span><strong>Completed:</strong> ${new Date(job.completionDate).toLocaleDateString()}</span>` : ''}
                                                            <span><strong>ASME Job:</strong> ${isASME ? 'True' : 'False'}</span>
                                                            <span><strong>Current Process:</strong> ${currentProcessName || 'Not Set'} (${job.currentProcessStep + 1} of ${job.requiredProcesses.length})</span>
                                                            ${assignedTo ? `<span><strong>Assigned To:</strong> ${assignedTo}</span>` : ''}
                                                            ${isOutsideProcess ? `<span><strong>Outside Process:</strong> Yes (${outsideProvider || 'N/A'})</span>` : '<span><strong>Outside Process:</strong> No</span>'}
                                                            <span><strong>Required Workflow:</strong> ${job.requiredProcesses.join(' &#8594; ') || 'Not Defined'}</span>
                                                            ${job.reworkCost !== undefined && job.reworkCost !== null && job.reworkCost > 0 ? `<span><strong>Rework Cost:</strong> $${job.reworkCost.toFixed(2)}</p>` : ''}
                                                            ${job.scrapCost !== undefined && job.scrapCost !== null && job.scrapCost > 0 ? `<span><strong>Scrap Cost:</strong> $${job.scrapCost.toFixed(2)}</span>` : ''}
                                                            ${job.isRework ? `<span><strong>Original Job ID:</strong> ${job.originalJobId || 'N/A'}</span>` : ''}
                                                            ${job.jobFilesFolderLocation ? `<span class="folder-location-item"><strong>Job Files Folder:</strong> ${job.jobFilesFolderLocation} <button class="open-folder-btn" data-path="${job.jobFilesFolderLocation}" title="Copy Job Folder Path"><i class="fas fa-folder-open"></i></button></span>` : ''}
                                                            ${job.dxfFilesFolderLocation ? `<span class="folder-location-item"><strong>DXF Files Folder:</strong> ${job.dxfFilesFolderLocation} <button class="open-folder-btn" data-path="${job.dxfFilesFolderLocation}" title="Copy DXF Folder Path"><i class="fas fa-folder-open"></i></button></span>` : ''}
                                                        </div>
                                                        ${job.visualReferenceImage || job.eDrawingReference || job.drawingPdfBase64 ? `
                                                            <div class="visual-reference-display">
                                                                <h4>Visual References:</h4>
                                                                ${job.visualReferenceImage ? `<img src="${job.visualReferenceImage}" alt="Job Visual Reference">` : ''}
                                                                ${job.drawingPdfBase64 ? `<span>PDF Drawing Attached</span>` : ''}
                                                                ${job.eDrawingReference ? `<span>Other Reference File: ${job.eDrawingReference}</span>` : ''}
                                                            </div>
                                                        ` : ''}
                                                        ${job.nonConformances && job.nonConformances.length > 0 ? `
                                                            <div class="non-conformances-display">
                                                                <h4><i class="fas fa-exclamation-circle"></i> Non-Conformances:</h4>
                                                                ${job.nonConformances.map(nc => `
                                                                    <div class="nc-item">
                                                                        <div class="nc-item-grid">
                                                                            <p><strong>NC ID:</strong> <span class="nc-id">${nc.id}</span></p>
                                                                            <p><strong>Status:</strong> ${nc.isClosed ? 'Completed' : 'Open'}</p>
                                                                            <p><strong>Caught In:</strong> ${nc.caughtProcess}</p>
                                                                            <p><strong>Caused By:</strong> ${nc.causedProcess}</p>
                                                                            <p><strong>Date:</strong> ${new Date(nc.date).toLocaleDateString()}</p>
                                                                            <p><strong>Action:</strong> ${nc.action}</p>
                                                                        </div>
                                                                        <p><strong>Details of Issue:</strong> ${nc.details}</p>
                                                                        ${nc.otherActionNotes ? `<p><strong>Other Action Notes:</strong> ${nc.otherActionNotes}</p>` : ''}
                                                                        ${nc.isClosed ? `
                                                                            <p><strong>Completion Date:</strong> ${new Date(nc.completionDate).toLocaleDateString()}</p>
                                                                            <p><strong>Completed By:</strong> ${nc.completedBy}</p>
                                                                            <p><strong>Completion Action:</strong> ${nc.completionAction}</p>
                                                                        ` : ''}
                                                                    </div>
                                                                `).join('')}
                                                            </div>
                                                        ` : ''}
                                                        ${job.shopNotes ? `<div class="job-notes-display"><strong>Shop Notes:</strong><br>${job.shopNotes.replace(/\n/g, '<br>')}</div>` : ''}
                                                        <div class="job-notes">
                                                            <textarea class="notes-textarea" placeholder="Add general notes here...">${job.notes || ''}</textarea>
                                                        </div>
                                                    </div>
                                                `;

                            const completeBtn = li.querySelector('.complete-btn');
                            const deleteBtn = li.querySelector('.delete-btn');
                            const editStatusBtn = li.querySelector('.edit-status-btn');
                            const detailsToggleBtn = li.querySelector('.details-toggle-btn');
                            const expandableContent = li.querySelector('.job-expandable-content');
                            const notesTextarea = li.querySelector('.notes-textarea');
                            const viewDrawingBtn = li.querySelector('.view-drawing-btn');
                            const editJobBtn = li.querySelector('.edit-job-btn');
                            const addShopNotesBtn = li.querySelector('.add-shop-notes-btn');
                            const viewNcDetailsBtn = li.querySelector('.view-nc-details-btn'); // New NC button
                            const openFolderBtns = li.querySelectorAll('.open-folder-btn'); // Select all new folder buttons

                            completeBtn.addEventListener('click', () => toggleComplete(job.id));
                            deleteBtn.addEventListener('click', () => deleteJob(job.id));

                            if (editStatusBtn) {
                                editStatusBtn.addEventListener('click', () => openEditStatusModal(job));
                            }

                            if (viewDrawingBtn) {
                                viewDrawingBtn.addEventListener('click', () => viewJobDrawing(job.id));
                            }

                            if (editJobBtn) {
                                editJobBtn.addEventListener('click', () => openEditJobModal(job));
                            }

                            if (addShopNotesBtn) {
                                addShopNotesBtn.addEventListener('click', () => openShopNotesModal(job));
                            }

                            if (viewNcDetailsBtn) { // Add listener for new NC button
                                viewNcDetailsBtn.addEventListener('click', () => openViewNcDetailsModal(job));
                            }

                            detailsToggleBtn.addEventListener('click', () => {
                                expandableContent.classList.toggle('visible');
                                if (expandableContent.classList.contains('visible')) {
                                    detailsToggleBtn.innerHTML = '<i class="fas fa-info-circle"></i> Hide Details';
                                } else {
                                    detailsToggleBtn.innerHTML = '<i class="fas fa-info-circle"></i> Show Details';
                                }
                            });

                            notesTextarea.addEventListener('input', (e) => {
                                job.notes = e.target.value;
                                saveData();
                            });

                            openFolderBtns.forEach(button => {
                                button.addEventListener('click', (e) => {
                                    const path = e.target.dataset.path || e.target.parentElement.dataset.path; // Get path from button or its parent span
                                    copyToClipboardAndNotify(path, `Folder path copied to clipboard: ${path}`);
                                });
                            });
                        }

                        return li;
                    };

                    // --- Job Actions ---

                    const openAddJobModal = (jobToPrepopulate = null) => {
                        currentJobBeingEdited = jobToPrepopulate;
                        addJobModalTitle.textContent = jobToPrepopulate ? `Edit Job: ${jobToPrepopulate.description}` : 'Add New Job';
                        modalAddJobBtn.textContent = jobToPrepopulate ? 'Save Changes' : 'Add Job';
                        modalAddJobBtn.classList.remove('add-btn-modal');
                        modalAddJobBtn.classList.add(jobToPrepopulate ? 'save-btn' : 'add-btn-modal');

                        modalJobDescriptionInput.value = jobToPrepopulate ? jobToPrepopulate.description : '';
                        modalJobNumberInput.value = jobToPrepopulate ? jobToPrepopulate.jobNumber : '';
                        modalCustomerNameInput.value = jobToPrepopulate ? jobToPrepopulate.customer : '';
                        modalDrawingNumberInput.value = jobToPrepopulate ? jobToPrepopulate.drawingNumber : '';
                        modalDueDateInput.value = jobToPrepopulate ? jobToPrepopulate.dueDate : '';
                        modalPriorityInput.value = jobToPrepopulate ? jobToPrepopulate.priority : 'low';
                        modalIsASMESelect.value = jobToPrepopulate ? (jobToPrepopulate.isASME ? 'true' : 'false') : 'false';
                        modalQuantityInput.value = jobToPrepopulate ? (jobToPrepopulate.quantity || 1) : '1';
                        modalIsRework.checked = jobToPrepopulate ? (jobToPrepopulate.isRework || false) : false;

                        // New fields
                        modalJobFilesFolderLocation.value = jobToPrepopulate ? jobToPrepopulate.jobFilesFolderLocation || '' : '';
                        modalDxfFilesFolderLocation.value = jobToPrepopulate ? jobToPrepopulate.dxfFilesFolderLocation || '' : '';

                        csvFileInput.value = '';

                        tempRequiredProcesses = jobToPrepopulate ? [...jobToPrepopulate.requiredProcesses] : [];
                        renderRequiredProcessesList();

                        visualReferenceImageInput.value = '';
                        if (jobToPrepopulate && jobToPrepopulate.visualReferenceImage) {
                            tempVisualReferenceImage = jobToPrepopulate.visualReferenceImage;
                            imagePreview.src = jobToPrepopulate.visualReferenceImage;
                            imagePreview.style.display = 'block';
                            imagePlaceholder.style.display = 'none';
                            clearImageBtn.style.display = 'inline-block';
                        } else {
                            tempVisualReferenceImage = '';
                            imagePreview.src = '#';
                            imagePreview.style.display = 'none';
                            imagePlaceholder.style.display = 'block';
                            clearImageBtn.style.display = 'none';
                        }

                        drawingPdfInput.value = '';
                        if (jobToPrepopulate && jobToPrepopulate.drawingPdfBase64) {
                            tempDrawingPdfBase64 = jobToPrepopulate.drawingPdfBase64;
                            tempDrawingPdfFileName = jobToPrepopulate.drawingPdfFileName || '';
                            pdfFileNameDisplay.textContent = tempDrawingPdfFileName ? `Selected: ${tempDrawingPdfFileName}` : 'PDF Drawing Attached';
                            pdfFileNameDisplay.style.display = 'block';
                            pdfPlaceholder.style.display = 'none';
                            clearPdfBtn.style.display = 'inline-block';
                        } else {
                            tempDrawingPdfBase64 = '';
                            tempDrawingPdfFileName = '';
                            pdfFileNameDisplay.textContent = '';
                            pdfFileNameDisplay.style.display = 'none';
                            pdfPlaceholder.style.display = 'block';
                            clearPdfBtn.style.display = 'none';
                        }

                        eDrawingReferenceInput.value = jobToPrepopulate ? jobToPrepopulate.eDrawingReference : '';

                        clearValidationHighlights(addJobModal); // Clear highlights on open
                        addJobModal.classList.add('visible');
                    };

                    const openEditJobModal = (job) => {
                        openAddJobModal(job); // Reuse the same function for editing
                    };


                    const closeAddJobModal = () => {
                        addJobModal.classList.remove('visible');
                        currentJobBeingEdited = null;
                        clearValidationHighlights(addJobModal); // Clear highlights on close
                        renderJobs();
                    };

                    const renderRequiredProcessesList = () => {
                        requiredProcessesList.innerHTML = '';
                        if (tempRequiredProcesses.length === 0) {
                            requiredProcessesList.innerHTML = '<p class="placeholder-text">Add processes to define this job\'s workflow.</p>';
                            // Also clear any validation highlight for the list itself
                            requiredProcessesValidationMessage.classList.remove('input-error');
                            hideModalValidationMessage(requiredProcessesValidationMessage);
                            return;
                        }

                        tempRequiredProcesses.forEach((processName, index) => {
                            const li = document.createElement('li');
                            li.classList.add('required-process-item');
                            li.dataset.index = index;
                            li.innerHTML = `
                                                    <span>${processName}</span>
                                                    <div class="required-process-item-actions">
                                                        <button class="move-up-btn" title="Move Up"><i class="fas fa-arrow-up"></i></button>
                                                        <button class="move-down-btn" title="Move Down"><i class="fas fa-arrow-down"></i></button>
                                                        <button class="remove-btn" title="Remove"><i class="fas fa-times"></i></button>
                                                    </div>
                                                `;

                            li.querySelector('.move-up-btn').addEventListener('click', () => moveProcessInList(index, -1));
                            li.querySelector('.move-down-btn').addEventListener('click', () => moveProcessInList(index, 1));
                            li.querySelector('.remove-btn').addEventListener('click', () => removeProcessFromList(index));
                            requiredProcessesList.appendChild(li);
                        });
                        // If list is populated, ensure no error is shown for it
                        requiredProcessesValidationMessage.classList.remove('input-error');
                        hideModalValidationMessage(requiredProcessesValidationMessage);
                    };

                    const addProcessToList = () => {
                        const selectedProcess = availableProcessesSelect.value;
                        if (selectedProcess && !tempRequiredProcesses.includes(selectedProcess)) {
                            tempRequiredProcesses.push(selectedProcess);
                            renderRequiredProcessesList();
                        } else if (selectedProcess && tempRequiredProcesses.includes(selectedProcess)) {
                            showMessage('Process already added to the list.', 'warning');
                        } else {
                            showMessage('Please select a process to add.', 'error');
                        }
                    };

                    const removeProcessFromList = (index) => {
                        tempRequiredProcesses.splice(index, 1);
                        renderRequiredProcessesList();
                    };

                    const moveProcessInList = (index, direction) => {
                        const newIndex = index + direction;
                        if (newIndex >= 0 && newIndex < tempRequiredProcesses.length) {
                            const [movedProcess] = tempRequiredProcesses.splice(index, 1);
                            tempRequiredProcesses.splice(newIndex, 0, movedProcess);
                            renderRequiredProcessesList();
                        }
                    };


                    const saveJob = () => {
                        // Validate required fields
                        const requiredInputs = [
                            modalJobDescriptionInput,
                            modalJobNumberInput,
                            modalCustomerNameInput
                        ];

                        const isFormValid = validateAndHighlight(requiredInputs, addJobModalValidationMessage);
                        const areProcessesAdded = tempRequiredProcesses.length > 0;

                        if (!areProcessesAdded) {
                            showModalValidationMessage(requiredProcessesValidationMessage, 'Please define at least one required process for the job.');
                        } else {
                            hideModalValidationMessage(requiredProcessesValidationMessage);
                        }

                        if (!isFormValid || !areProcessesAdded) {
                            return; // Stop if validation fails
                        }

                        const description = modalJobDescriptionInput.value.trim();
                        const jobNumber = modalJobNumberInput.value.trim();
                        const customer = modalCustomerNameInput.value.trim();
                        const drawingNumber = modalDrawingNumberInput.value.trim();
                        const dueDate = modalDueDateInput.value;
                        const priority = modalPriorityInput.value;
                        const isASME = modalIsASMESelect.value === 'true';
                        const quantity = parseInt(modalQuantityInput.value, 10);
                        const eDrawingReference = eDrawingReferenceInput.value.trim();
                        const isRework = modalIsRework.checked;
                        const jobFilesFolderLocation = modalJobFilesFolderLocation.value.trim(); // New
                        const dxfFilesFolderLocation = modalDxfFilesFolderLocation.value.trim(); // New

                        // Quantity validation (already handled by input type="number" min="1" but good to double check)
                        if (isNaN(quantity) || quantity <= 0) {
                            showMessage('Quantity must be a positive number.', 'error');
                            return;
                        }

                        if (currentJobBeingEdited) {
                            const jobIndex = jobs.findIndex(j => j.id === currentJobBeingEdited.id);
                            if (jobIndex !== -1) {
                                jobs[jobIndex].description = description;
                                jobs[jobIndex].jobNumber = jobNumber;
                                jobs[jobIndex].customer = customer;
                                jobs[jobIndex].drawingNumber = drawingNumber;
                                jobs[jobIndex].dueDate = dueDate;
                                jobs[jobIndex].priority = priority;
                                jobs[jobIndex].isASME = isASME;
                                jobs[jobIndex].quantity = quantity;
                                jobs[jobIndex].requiredProcesses = [...tempRequiredProcesses];
                                jobs[jobIndex].visualReferenceImage = tempVisualReferenceImage;
                                jobs[jobIndex].eDrawingReference = eDrawingReference;
                                jobs[jobIndex].drawingPdfBase64 = tempDrawingPdfBase64;
                                jobs[jobIndex].drawingPdfFileName = tempDrawingPdfFileName;
                                jobs[jobIndex].shopNotes = jobs[jobIndex].shopNotes || '';
                                jobs[jobIndex].reworkQuantity = jobs[jobIndex].reworkQuantity || 0;
                                jobs[jobIndex].reworkCost = jobs[jobIndex].reworkCost || 0;
                                jobs[jobIndex].scrapCost = jobs[jobIndex].scrapCost || 0;
                                jobs[jobIndex].nonConformances = jobs[jobIndex].nonConformances || [];
                                jobs[jobIndex].isRework = isRework; // Update rework status
                                jobs[jobIndex].jobFilesFolderLocation = jobFilesFolderLocation; // New
                                jobs[jobIndex].dxfFilesFolderLocation = dxfFilesFolderLocation; // New
                                // originalJobId is only set on creation of a rework job, not edited here
                                showMessage('Job updated successfully!', 'success');
                            }
                        } else {
                            const newJob = {
                                id: Date.now(),
                                description: description,
                                jobNumber: jobNumber,
                                customer: customer,
                                drawingNumber: drawingNumber,
                                dueDate: dueDate,
                                priority: priority,
                                isASME: isASME,
                                quantity: quantity,
                                requiredProcesses: [...tempRequiredProcesses],
                                currentProcessStep: 0,
                                addedDate: new Date().toISOString().slice(0, 10),
                                completed: false,
                                notes: '',
                                completionDate: null,
                                visualReferenceImage: tempVisualReferenceImage,
                                eDrawingReference: eDrawingReference,
                                drawingPdfBase64: tempDrawingPdfBase64,
                                drawingPdfFileName: tempDrawingPdfFileName,
                                shopNotes: '',
                                reworkQuantity: 0,
                                reworkCost: 0,
                                scrapCost: 0,
                                nonConformances: [],
                                isRework: isRework,
                                originalJobId: null, // Set if created from rework button
                                jobFilesFolderLocation: jobFilesFolderLocation, // New
                                dxfFilesFolderLocation: dxfFilesFolderLocation // New
                            };
                            jobs.push(newJob);
                            showMessage('Job added successfully!', 'success');
                        }

                        closeAddJobModal();
                        renderJobs();
                    };

                    const generateCashJobNumber = () => {
                        const now = new Date();
                        const year = now.getFullYear();
                        const month = String(now.getMonth() + 1).padStart(2, '0');
                        const day = String(now.getDate()).padStart(2, '0');
                        const hours = String(now.getHours()).padStart(2, '0');
                        const minutes = String(now.getMinutes()).padStart(2, '0');
                        const seconds = String(now.getSeconds()).padStart(2, '0');

                        const cashJobNo = `CASH-${year}${month}${day}-${hours}${minutes}${seconds}`;
                        modalJobNumberInput.value = cashJobNo;
                        modalCustomerNameInput.value = "Cash Customer";
                        // Clear validation highlight if generated
                        modalJobNumberInput.classList.remove('input-error');
                        modalCustomerNameInput.classList.remove('input-error');
                        hideModalValidationMessage(addJobModalValidationMessage);
                    };

                    visualReferenceImageInput.addEventListener('change', (e) => {
                        const file = e.target.files[0];
                        if (file) {
                            const reader = new FileReader();
                            reader.onload = (event) => {
                                tempVisualReferenceImage = event.target.result;
                                imagePreview.src = tempVisualReferenceImage;
                                imagePreview.style.display = 'block';
                                imagePlaceholder.style.display = 'none';
                                clearImageBtn.style.display = 'inline-block';
                            };
                            reader.readAsDataURL(file);
                        } else {
                            tempVisualReferenceImage = '';
                            imagePreview.src = '#';
                            imagePreview.style.display = 'none';
                            imagePlaceholder.style.display = 'block';
                            clearImageBtn.style.display = 'none';
                        }
                    });

                    clearImageBtn.addEventListener('click', () => {
                        visualReferenceImageInput.value = '';
                        tempVisualReferenceImage = '';
                        imagePreview.src = '#';
                        imagePreview.style.display = 'none';
                        imagePlaceholder.style.display = 'block';
                        clearImageBtn.style.display = 'none';
                    });

                    drawingPdfInput.addEventListener('change', (e) => {
                        const file = e.target.files[0];
                        if (file) {
                            const reader = new FileReader();
                            reader.onload = (event) => {
                                tempDrawingPdfBase64 = event.target.result;
                                tempDrawingPdfFileName = file.name;
                                pdfFileNameDisplay.textContent = `Selected: ${file.name}`;
                                pdfFileNameDisplay.style.display = 'block';
                                pdfPlaceholder.style.display = 'none';
                                clearPdfBtn.style.display = 'inline-block';
                            };
                            reader.readAsDataURL(file);
                        } else {
                            tempDrawingPdfBase64 = '';
                            tempDrawingPdfFileName = '';
                            pdfFileNameDisplay.textContent = '';
                            pdfFileNameDisplay.style.display = 'none';
                            pdfPlaceholder.style.display = 'block';
                            clearPdfBtn.style.display = 'none';
                        }
                    });

                    clearPdfBtn.addEventListener('click', () => {
                        drawingPdfInput.value = '';
                        tempDrawingPdfBase64 = '';
                        tempDrawingPdfFileName = '';
                        pdfFileNameDisplay.textContent = '';
                        pdfFileNameDisplay.style.display = 'none';
                        pdfPlaceholder.style.display = 'block';
                        clearPdfBtn.style.display = 'none';
                    });


                    const handleCsvUpload = () => {
                        const file = csvFileInput.files[0];
                        if (!file) {
                            showMessage('Please select a CSV file to upload.', 'error');
                            return;
                        }

                        const reader = new FileReader();
                        reader.onload = (e) => {
                            const text = e.target.result;
                            parseCsvAndAddJobs(text);
                        };
                        reader.onerror = () => {
                            showMessage('Error reading file.', 'error');
                        };
                        reader.readAsText(file);
                    };

                    const parseCsvAndAddJobs = (csvText) => {
                        const lines = csvText.split('\n').filter(line => line.trim() !== '');
                        if (lines.length === 0) {
                            showMessage('CSV file is empty.', 'error');
                            return;
                        }

                        const headers = lines[0].split(',').map(header => header.trim());
                        const validPriorities = ['low', 'medium', 'high'];
                        let jobsAddedCount = 0;
                        let errorsFound = false;

                        for (let i = 1; i < lines.length; i++) {
                            const values = lines[i].split(',').map(value => value.trim());
                            if (values.length !== headers.length) {
                                console.warn(`Skipping row ${i + 1}: Mismatched column count.`);
                                errorsFound = true;
                                continue;
                            }

                            const jobData = {};
                            headers.forEach((header, index) => {
                                jobData[header] = values[index];
                            });

                            if (!jobData.description) {
                                console.warn(`Skipping row ${i + 1}: Missing required 'description'.`);
                                errorsFound = true;
                                continue;
                            }
                            if (!jobData.jobNumber) {
                                console.warn(`Skipping row ${i + 1}: Missing required 'jobNumber'.`);
                                errorsFound = true;
                                continue;
                            }
                            if (!jobData.customer) {
                                console.warn(`Skipping row ${i + 1}: Missing required 'customer'.`);
                                errorsFound = true;
                                continue;
                            }
                            if (!jobData.priority || !validPriorities.includes(jobData.priority.toLowerCase())) {
                                console.warn(`Skipping row ${i + 1}: Invalid or missing 'priority'. Must be low, medium, or high.`);
                                errorsFound = true;
                                continue;
                            }
                            if (jobData.isASME === undefined || !['true', 'false'].includes(jobData.isASME.toLowerCase())) {
                                console.warn(`Skipping row ${i + 1}: Invalid or missing 'isASME'. Must be true or false.`);
                                errorsFound = true;
                                continue;
                            }
                            if (!jobData.requiredProcesses) {
                                console.warn(`Skipping row ${i + 1}: Missing required 'requiredProcesses'.`);
                                errorsFound = true;
                                continue;
                            }
                            if (jobData.quantity && (isNaN(parseInt(jobData.quantity, 10)) || parseInt(jobData.quantity, 10) <= 0)) {
                                console.warn(`Skipping row ${i + 1}: Invalid 'quantity'. Must be a positive number.`);
                                errorsFound = true;
                                continue;
                            }
                            if (jobData.reworkQuantity && (isNaN(parseInt(jobData.reworkQuantity, 10)) || parseInt(jobData.reworkQuantity, 10) < 0)) {
                                console.warn(`Skipping row ${i + 1}: Invalid 'reworkQuantity'. Must be a non-negative number.`);
                                errorsFound = true;
                                continue;
                            }


                            const parsedRequiredProcesses = jobData.requiredProcesses.split(',').map(p => p.trim()).filter(p => p !== '');
                            if (parsedRequiredProcesses.length === 0) {
                                console.warn(`Skipping row ${i + 1}: 'requiredProcesses' is empty.`);
                                errorsFound = true;
                                continue;
                            }
                            const invalidProcesses = parsedRequiredProcesses.filter(pName => !processOptions.some(po => po.name === pName));
                            if (invalidProcesses.length > 0) {
                                console.warn(`Skipping row ${i + 1}: Contains invalid processes: ${invalidProcesses.join(', ')}.`);
                                errorsFound = true;
                                continue;
                            }


                            const newJob = {
                                id: Date.now() + i,
                                description: jobData.description,
                                jobNumber: jobData.jobNumber || '',
                                customer: jobData.customer || '',
                                drawingNumber: jobData.drawingNumber || '',
                                dueDate: jobData.dueDate || '',
                                priority: jobData.priority.toLowerCase(),
                                isASME: jobData.isASME.toLowerCase() === 'true',
                                quantity: jobData.quantity ? parseInt(jobData.quantity, 10) : 1,
                                requiredProcesses: parsedRequiredProcesses,
                                currentProcessStep: 0,
                                addedDate: new Date().toISOString().slice(0, 10),
                                completed: false,
                                notes: '',
                                completionDate: null,
                                visualReferenceImage: jobData.visualReferenceImage || '',
                                eDrawingReference: jobData.eDrawingReference || '',
                                drawingPdfBase64: jobData.drawingPdfBase64 || '',
                                drawingPdfFileName: jobData.drawingPdfFileName || '',
                                shopNotes: '',
                                reworkQuantity: 0,
                                reworkCost: 0,
                                scrapCost: 0,
                                nonConformances: [],
                                isRework: jobData.isRework ? jobData.isRework.toLowerCase() === 'true' : false,
                                originalJobId: jobData.originalJobId ? parseInt(jobData.originalJobId, 10) : null,
                                jobFilesFolderLocation: jobData.jobFilesFolderLocation || '', // New
                                dxfFilesFolderLocation: jobData.dxfFilesFolderLocation || '' // New
                            };
                            jobs.push(newJob);
                            jobsAddedCount++;
                        }

                        if (jobsAddedCount > 0) {
                            showMessage(`${jobsAddedCount} job(s) imported successfully!`, 'success');
                            closeAddJobModal();
                            renderJobs();
                        } else if (errorsFound) {
                            showMessage('No jobs imported. Please check CSV format and required fields in console.', 'error');
                        } else {
                            showMessage('No valid jobs found in CSV.', 'warning');
                        }
                    };


                    const toggleComplete = (id) => {
                        const job = jobs.find(j => j.id === id);
                        if (job) {
                            if (job.completed) {
                                job.completed = false;
                                job.completionDate = null;
                                job.currentProcessStep = job.requiredProcesses.length > 0 ? job.requiredProcesses.length - 1 : 0;
                                showMessage(`Job "${job.description}" moved back to active.`, 'success');
                            } else {
                                if (job.currentProcessStep < job.requiredProcesses.length - 1) {
                                    job.currentProcessStep++;
                                    showMessage(`Job "${job.description}" moved to "${job.requiredProcesses[job.currentProcessStep]}".`, 'success');
                                } else {
                                    job.completed = true;
                                    job.completionDate = new Date().toISOString().slice(0, 10);
                                    showMessage(`Job "${job.description}" marked as completed.`, 'success');
                                }
                            }
                            renderJobs();
                        }
                    };


                    const deleteJob = (id) => {
                        if (window.confirm('Are you sure you want to delete this job?')) {
                            const jobDescription = jobs.find(j => j.id === id)?.description || 'Job';
                            jobs = jobs.filter(j => j.id !== id);
                            showMessage(`${jobDescription} deleted successfully.`, 'success');
                            renderJobs();
                        }
                    };

                    // --- Modal Functions for Managing Job Workflow (formerly Edit Status) ---

                    const openEditStatusModal = (job) => {
                        currentJobToEdit = job;
                        editJobDescriptionDisplay.value = job.description;

                        // Populate cost inputs
                        editReworkQuantityInput.value = job.reworkQuantity !== undefined && job.reworkQuantity !== null ? job.reworkQuantity : 0; // New
                        editReworkCostInput.value = job.reworkCost !== undefined && job.reworkCost !== null ? job.reworkCost.toFixed(2) : '0.00';
                        editScrapCostInput.value = job.scrapCost !== undefined && job.scrapCost !== null ? job.scrapCost.toFixed(2) : '0.00';


                        updateEditStatusModalDisplay();

                        editStatusModal.classList.add('visible');
                    };

                    const updateEditStatusModalDisplay = () => {
                        if (!currentJobToEdit) return;

                        const job = currentJobToEdit;
                        const currentProcessName = job.requiredProcesses[job.currentProcessStep];
                        const currentProcessObj = processOptions.find(p => p.name === currentProcessName);

                        editCurrentProcessDisplay.value = currentProcessName || 'Not Set';
                        editAssignedToDisplay.value = currentProcessObj ? (currentProcessObj.defaultAssignedTo || 'N/A') : 'N/A';
                        editIsOutsideProcessDisplay.value = currentProcessObj ? (currentProcessObj.defaultIsOutsideProcess ? 'Yes' : 'No') : 'N/A';
                        editOutsideProviderDisplay.value = currentProcessObj && currentProcessObj.defaultIsOutsideProcess ? (currentProcessObj.defaultOutsideProvider || 'N/A') : '';

                        if (currentProcessObj && currentProcessObj.defaultIsOutsideProcess) {
                            editOutsideProviderDisplayRow.classList.remove('hidden-input');
                        } else {
                            editOutsideProviderDisplayRow.classList.add('hidden-input');
                            // editOutsideProviderDisplayInput.value = ''; // This line is not needed here as it's a display field
                        }

                        prevStageBtn.disabled = job.currentProcessStep <= 0;
                        nextStageBtn.disabled = job.currentProcessStep >= job.requiredProcesses.length - 1;
                        toggleCompleteBtn.textContent = job.completed ? 'Undo Completion' : 'Complete Job';
                        toggleCompleteBtn.classList.toggle('complete-btn', !job.completed);
                        toggleCompleteBtn.classList.toggle('warning-color', job.completed);
                    };

                    const saveJobCosts = () => {
                        if (currentJobToEdit) {
                            currentJobToEdit.reworkQuantity = parseInt(editReworkQuantityInput.value, 10) || 0; // New
                            currentJobToEdit.reworkCost = parseFloat(editReworkCostInput.value) || 0;
                            currentJobToEdit.scrapCost = parseFloat(editScrapCostInput.value) || 0;
                            showMessage(`Costs for "${currentJobToEdit.description}" saved.`, 'success');
                            saveData();
                            renderJobs(); // Re-render to show updated costs
                        }
                    };


                    const closeEditStatusModal = () => {
                        editStatusModal.classList.remove('visible');
                        currentJobToEdit = null;
                    };

                    const moveToNextStage = () => {
                        if (currentJobToEdit && currentJobToEdit.currentProcessStep < currentJobToEdit.requiredProcesses.length - 1) {
                            currentJobToEdit.currentProcessStep++;
                            showMessage(`Job "${currentJobToEdit.description}" moved to "${currentJobToEdit.requiredProcesses[currentJobToEdit.currentProcessStep]}".`, 'success');
                            updateEditStatusModalDisplay();
                            saveData();
                        }
                    };

                    const moveToPreviousStage = () => {
                        if (currentJobToEdit && currentJobToEdit.currentProcessStep > 0) {
                            currentJobToEdit.currentProcessStep--;
                            showMessage(`Job "${currentJobToEdit.description}" moved back to "${currentJobToEdit.requiredProcesses[currentJobToEdit.currentProcessStep]}".`, 'success');
                            updateEditStatusModalDisplay();
                            saveData();
                        }
                    };

                    const toggleJobCompletionFromModal = () => {
                        if (currentJobToEdit) {
                            toggleComplete(currentJobToEdit.id);
                            updateEditStatusModalDisplay();
                        }
                    };

                    // --- Non-Conformance (NC) Modal Functions ---
                    const openSubmitNcModal = (job, nc = null) => {
                        currentJobForNCSubmission = job;
                        currentNcToEdit = nc;

                        submitNcModalTitle.textContent = nc ? `Edit Non-Conformance (NC ID: ${nc.id})` : `Submit Non-Conformance for Job: ${job.description}`;
                        submitNcDetailsBtn.textContent = nc ? 'Save Changes' : 'Submit NC';
                        submitNcDetailsBtn.classList.remove('add-btn-modal');
                        submitNcDetailsBtn.classList.add('save-btn');

                        // Clear or pre-populate inputs
                        ncProcessCaughtSelect.value = nc ? nc.caughtProcess : '';
                        ncProcessCausedSelect.value = nc ? nc.causedProcess : '';
                        ncDetailsInput.value = nc ? nc.details : '';
                        ncActionSelect.value = nc ? nc.action : '';
                        ncOtherActionNotesInput.value = nc ? nc.otherActionNotes : '';

                        // Handle new status field and completion fields visibility
                        if (nc) { // If editing an existing NC
                            ncStatusField.classList.remove('hidden-input'); // Show status field
                            ncStatusSelect.value = nc.isClosed ? 'completed' : 'open'; // Set current status

                            // Show/hide completion fields based on current status
                            if (nc.isClosed) {
                                ncCompletionFields.forEach(row => row.classList.remove('hidden-input'));
                                ncCompletionDate.value = nc.completionDate || '';
                                ncCompletedBy.value = nc.completedBy || '';
                                ncCompletionAction.value = nc.completionAction || '';
                            } else {
                                ncCompletionFields.forEach(row => row.classList.add('hidden-input'));
                                ncCompletionDate.value = '';
                                ncCompletedBy.value = '';
                                ncCompletionAction.value = '';
                            }

                            // Disable fields if NC is already completed
                            if (nc.isClosed) {
                                ncProcessCaughtSelect.disabled = true;
                                ncProcessCausedSelect.disabled = true;
                                ncDetailsInput.disabled = true;
                                ncActionSelect.disabled = true;
                                ncOtherActionNotesInput.disabled = true;
                                ncStatusSelect.disabled = true; // Disable status select if completed
                                ncCompletionDate.disabled = true;
                                ncCompletedBy.disabled = true;
                                ncCompletionAction.disabled = true;
                                submitNcDetailsBtn.classList.add('hidden-input'); // Hide Save Changes
                            } else {
                                ncProcessCaughtSelect.disabled = false;
                                ncProcessCausedSelect.disabled = false;
                                ncDetailsInput.disabled = false;
                                ncActionSelect.disabled = false;
                                ncOtherActionNotesInput.disabled = false;
                                ncStatusSelect.disabled = false; // Enable status select if open
                                ncCompletionDate.disabled = false;
                                ncCompletedBy.disabled = false;
                                ncCompletionAction.disabled = false;
                                submitNcDetailsBtn.classList.remove('hidden-input'); // Show Save Changes
                            }

                        } else { // If adding a new NC
                            ncStatusField.classList.add('hidden-input'); // Hide status field for new NC
                            ncStatusSelect.value = 'open'; // Default new NC to open
                            ncCompletionFields.forEach(row => row.classList.add('hidden-input'));
                            ncCompletionDate.value = '';
                            ncCompletedBy.value = '';
                            ncCompletionAction.value = '';
                            // Ensure fields are enabled for new NC
                            ncProcessCaughtSelect.disabled = false;
                            ncProcessCausedSelect.disabled = false;
                            ncDetailsInput.disabled = false;
                            ncActionSelect.disabled = false;
                            ncOtherActionNotesInput.disabled = false;
                        }

                        // Handle conditional visibility of other notes and rework button (existing logic)
                        if (ncActionSelect.value === 'Other Request') {
                            ncOtherActionNotesInput.classList.remove('hidden-input');
                        } else {
                            ncOtherActionNotesInput.classList.add('hidden-input');
                        }

                        // Rework button visibility (only if not already closed)
                        if (ncActionSelect.value === 'Rework Required' && (!nc || !nc.isClosed)) {
                            createReworkJobBtn.classList.remove('hidden-input');
                        } else {
                            createReworkJobBtn.classList.add('hidden-input');
                        }

                        clearValidationHighlights(submitNcModal); // Clear highlights on open
                        submitNcModal.classList.add('visible');
                    };

                    const submitNonConformance = () => {
                        const caughtProcess = ncProcessCaughtSelect.value;
                        const causedProcess = ncProcessCausedSelect.value;
                        const details = ncDetailsInput.value.trim();
                        const action = ncActionSelect.value;
                        const otherActionNotes = ncOtherActionNotesInput.value.trim();
                        const isClosed = ncStatusSelect.value === 'completed'; // Get status from new select

                        let completionDate = null;
                        let completedBy = '';
                        let completionAction = '';

                        // Required fields for NC
                        const requiredInputs = [
                            ncProcessCaughtSelect,
                            ncProcessCausedSelect,
                            ncDetailsInput,
                            ncActionSelect
                        ];

                        if (action === 'Other Request') {
                            requiredInputs.push(ncOtherActionNotesInput);
                        }

                        if (isClosed) {
                            completionDate = ncCompletionDate.value;
                            completedBy = ncCompletedBy.value.trim();
                            completionAction = ncCompletionAction.value.trim();
                            requiredInputs.push(ncCompletionDate, ncCompletedBy, ncCompletionAction);
                        }

                        if (!validateAndHighlight(requiredInputs, submitNcModalValidationMessage)) {
                            return; // Stop if validation fails
                        }


                        if (currentNcToEdit) {
                            // Editing an existing NC
                            const jobIndex = jobs.findIndex(j => j.id === currentJobForNCSubmission.id);
                            if (jobIndex !== -1) {
                                const ncIndex = jobs[jobIndex].nonConformances.findIndex(n => n.id === currentNcToEdit.id);
                                if (ncIndex !== -1) {
                                    jobs[jobIndex].nonConformances[ncIndex].caughtProcess = caughtProcess;
                                    jobs[jobIndex].nonConformances[ncIndex].causedProcess = causedProcess;
                                    jobs[jobIndex].nonConformances[ncIndex].details = details;
                                    jobs[jobIndex].nonConformances[ncIndex].action = action;
                                    jobs[jobIndex].nonConformances[ncIndex].otherActionNotes = action === 'Other Request' ? otherActionNotes : '';
                                    jobs[jobIndex].nonConformances[ncIndex].isClosed = isClosed; // Update status
                                    jobs[jobIndex].nonConformances[ncIndex].completionDate = completionDate;
                                    jobs[jobIndex].nonConformances[ncIndex].completedBy = completedBy;
                                    jobs[jobIndex].nonConformances[ncIndex].completionAction = completionAction;
                                    showMessage(`Non-Conformance (ID: ${currentNcToEdit.id}) updated.`, 'success');
                                }
                            }
                        } else if (currentJobForNCSubmission) {
                            // Submitting a new NC
                            const newNc = {
                                id: Date.now(), // Unique ID for each NC
                                caughtProcess,
                                causedProcess,
                                details,
                                action,
                                otherActionNotes: action === 'Other Request' ? otherActionNotes : '',
                                date: new Date().toISOString().slice(0, 10),
                                isClosed: false, // New NCs are always open by default
                                completionDate: null,
                                completedBy: '',
                                completionAction: ''
                            };
                            currentJobForNCSubmission.nonConformances.push(newNc);
                            showMessage(`Non-Conformance submitted for "${currentJobForNCSubmission.description}".`, 'success');
                        }
                        saveData();
                        closeSubmitNcModal();
                        renderJobs(); // Re-render to display new NC
                        // If manage all NCs modal is open, re-render it
                        if (manageAllNcModal.classList.contains('visible')) {
                            openManageAllNcModal(ncSearchInput.value, ncFilterCaughtInSelect.value, ncFilterCausedBySelect.value, ncFilterStatusSelect.value); // Re-open to refresh list with current filters
                        }
                    };

                    // Removed completeNonConformance() as it's no longer needed

                    const createReworkJob = () => {
                        if (currentJobForNCSubmission) {
                            const originalJob = currentJobForNCSubmission;
                            const newJobDescription = `REWORK: ${originalJob.description}`;
                            const newJobNumber = `${originalJob.jobNumber}-R${originalJob.nonConformances.length}`; // Simple rework job number

                            const newReworkJob = {
                                id: Date.now(),
                                description: newJobDescription,
                                jobNumber: newJobNumber,
                                customer: originalJob.customer,
                                drawingNumber: originalJob.drawingNumber,
                                dueDate: originalJob.dueDate, // Keep original due date or set new? For now, keep.
                                priority: originalJob.priority,
                                isASME: originalJob.isASME,
                                quantity: originalJob.quantity, // Rework quantity could be different, but default to original
                                requiredProcesses: [...originalJob.requiredProcesses], // Start from first process
                                currentProcessStep: 0,
                                addedDate: new Date().toISOString().slice(0, 10),
                                completed: false,
                                notes: `Rework job created from NC on original job ID: ${originalJob.id}. Original description: ${originalJob.description}.`,
                                completionDate: null,
                                visualReferenceImage: originalJob.visualReferenceImage,
                                eDrawingReference: originalJob.eDrawingReference,
                                drawingPdfBase64: originalJob.drawingPdfBase64,
                                drawingPdfFileName: originalJob.drawingPdfFileName,
                                shopNotes: `Rework for NC on job ${originalJob.jobNumber}.`,
                                reworkQuantity: 0, // This is a new job, so its rework quantity starts at 0
                                reworkCost: 0,
                                scrapCost: 0,
                                nonConformances: [],
                                isRework: true, // Mark as rework job
                                originalJobId: originalJob.id, // Link to original job
                                jobFilesFolderLocation: originalJob.jobFilesFolderLocation, // Carry over folder locations
                                dxfFilesFolderLocation: originalJob.dxfFilesFolderLocation // Carry over folder locations
                            };

                            jobs.push(newReworkJob);
                            showMessage(`Rework job "${newReworkJob.description}" created!`, 'success');
                            closeSubmitNcModal();
                            renderJobs();
                        }
                    };


                    const closeSubmitNcModal = () => {
                        submitNcModal.classList.remove('visible');
                        currentJobForNCSubmission = null;
                        currentNcToEdit = null;
                        // Re-enable fields when closing the modal
                        ncProcessCaughtSelect.disabled = false;
                        ncProcessCausedSelect.disabled = false;
                        ncDetailsInput.disabled = false;
                        ncActionSelect.disabled = false;
                        ncOtherActionNotesInput.disabled = false;
                        ncStatusSelect.disabled = false; // Ensure status select is enabled
                        ncCompletionDate.disabled = false;
                        ncCompletedBy.disabled = false;
                        ncCompletionAction.disabled = false;
                        clearValidationHighlights(submitNcModal); // Clear highlights on close
                    };

                    // Listener for NC Action Select
                    ncActionSelect.addEventListener('change', () => {
                        if (ncActionSelect.value === 'Other Request') {
                            ncOtherActionNotesInput.classList.remove('hidden-input');
                        } else {
                            ncOtherActionNotesInput.classList.add('hidden-input');
                            ncOtherActionNotesInput.value = ''; // Clear if hidden
                        }

                        if (ncActionSelect.value === 'Rework Required' && (!currentNcToEdit || !currentNcToEdit.isClosed)) {
                            createReworkJobBtn.classList.remove('hidden-input');
                        } else {
                            createReworkJobBtn.classList.add('hidden-input');
                        }
                        ncActionSelect.classList.remove('input-error'); // Clear highlight on change
                        hideModalValidationMessage(submitNcModalValidationMessage);
                    });

                    // New listener for NC Status Select
                    ncStatusSelect.addEventListener('change', () => {
                        const isCompleted = ncStatusSelect.value === 'completed';
                        if (isCompleted) {
                            ncCompletionFields.forEach(row => row.classList.remove('hidden-input'));
                            // Pre-fill date and user if empty
                            if (!ncCompletionDate.value) {
                                ncCompletionDate.value = new Date().toISOString().slice(0, 10);
                            }
                            // You might want to pre-fill ncCompletedBy with a logged-in user's name if available
                        } else {
                            ncCompletionFields.forEach(row => row.classList.add('hidden-input'));
                            ncCompletionDate.value = '';
                            ncCompletedBy.value = '';
                            ncCompletionAction.value = '';
                        }
                        ncStatusSelect.classList.remove('input-error'); // Clear highlight on change
                        hideModalValidationMessage(submitNcModalValidationMessage);
                    });


                    // --- View NC Details Modal Functions ---
                    const openViewNcDetailsModal = (job) => {
                        currentJobForNCView = job;
                        ncJobDescriptionDisplay.textContent = job.description;
                        ncDetailsList.innerHTML = ''; // Clear previous details

                        if (job.nonConformances && job.nonConformances.length > 0) {
                            ncDetailsPlaceholder.style.display = 'none';
                            job.nonConformances.forEach((nc, index) => {
                                const li = document.createElement('li');
                                li.classList.add('nc-item'); // Reuse existing nc-item style or create new
                                li.innerHTML = `
                                                        <div class="nc-item-grid">
                                                            <p><strong>NC ID:</strong> <span class="nc-id">${nc.id}</span></p>
                                                            <p><strong>Status:</strong> ${nc.isClosed ? 'Completed' : 'Open'}</p>
                                                            <p><strong>Caught In:</strong> ${nc.caughtProcess}</p>
                                                            <p><strong>Caused By:</strong> ${nc.causedProcess}</p>
                                                            <p><strong>Date:</strong> ${new Date(nc.date).toLocaleDateString()}</p>
                                                            <p><strong>Action:</strong> ${nc.action}</p>
                                                        </div>
                                                        <p><strong>Details of Issue:</strong> ${nc.details}</p>
                                                        ${nc.otherActionNotes ? `<p><strong>Other Action Notes:</strong> ${nc.otherActionNotes}</p>` : ''}
                                                        ${nc.isClosed ? `
                                                            <p><strong>Completion Date:</strong> ${new Date(nc.completionDate).toLocaleDateString()}</p>
                                                            <p><strong>Completed By:</strong> ${nc.completedBy}</p>
                                                            <p><strong>Completion Action:</strong> ${nc.completionAction}</p>
                                                        ` : ''}
                                                    `;
                                ncDetailsList.appendChild(li);
                            });
                        } else {
                            ncDetailsPlaceholder.style.display = 'block';
                        }

                        viewNcDetailsModal.classList.add('visible');
                    };

                    const closeViewNcDetailsModal = () => {
                        viewNcDetailsModal.classList.remove('visible');
                        currentJobForNCView = null;
                    };

                    // Removed openViewSingleNcDetailsModal as the "Details" button is gone


                    // --- Manage All NCs Modal Functions ---
                    const openManageAllNcModal = (searchTerm = '', filterCaughtIn = '', filterCausedBy = '', filterStatus = 'open') => { // Default to 'open'
                        allNcList.innerHTML = ''; // Clear previous list

                        // Set filter values in the UI elements
                        ncSearchInput.value = searchTerm;
                        ncFilterCaughtInSelect.value = filterCaughtIn;
                        ncFilterCausedBySelect.value = filterCausedBy;
                        ncFilterStatusSelect.value = filterStatus; // Set the new status filter

                        let allNonConformances = [];
                        jobs.forEach(job => {
                            if (job.nonConformances && job.nonConformances.length > 0) {
                                job.nonConformances.forEach(nc => {
                                    allNonConformances.push({
                                        jobId: job.id,
                                        jobDescription: job.description,
                                        jobNumber: job.jobNumber,
                                        ...nc
                                    });
                                });
                            }
                        });

                        // Apply filters
                        if (searchTerm) {
                            const lowerCaseSearchTerm = searchTerm.toLowerCase();
                            allNonConformances = allNonConformances.filter(nc =>
                                String(nc.id).toLowerCase().includes(lowerCaseSearchTerm) ||
                                nc.jobDescription.toLowerCase().includes(lowerCaseSearchTerm) ||
                                nc.jobNumber.toLowerCase().includes(lowerCaseSearchTerm) ||
                                nc.caughtProcess.toLowerCase().includes(lowerCaseSearchTerm) ||
                                nc.causedProcess.toLowerCase().includes(lowerCaseSearchTerm) ||
                                nc.details.toLowerCase().includes(lowerCaseSearchTerm) ||
                                nc.action.toLowerCase().includes(lowerCaseSearchTerm) ||
                                (nc.otherActionNotes && nc.otherActionNotes.toLowerCase().includes(lowerCaseSearchTerm))
                            );
                        }

                        if (filterCaughtIn) {
                            allNonConformances = allNonConformances.filter(nc => nc.caughtProcess === filterCaughtIn);
                        }
                        if (filterCausedBy) {
                            allNonConformances = allNonConformances.filter(nc => nc.causedProcess === filterCausedBy);
                        }

                        // Apply new status filter
                        if (filterStatus === 'open') {
                            allNonConformances = allNonConformances.filter(nc => !nc.isClosed);
                        } else if (filterStatus === 'completed') {
                            allNonConformances = allNonConformances.filter(nc => nc.isClosed);
                        }
                        // 'all' means no additional filtering by status


                        if (allNonConformances.length > 0) {
                            allNonConformances.sort((a, b) => new Date(b.date) - new Date(a.date)); // Sort by most recent first

                            allNonConformances.forEach(nc => {
                                const li = document.createElement('li');
                                li.classList.add('nc-list-item');
                                li.innerHTML = `
                                                        <div class="nc-main-details-grid">
                                                            <p><strong>NC ID:</strong> <span class="nc-id">${nc.id}</span></p>
                                                            <p><strong>Status:</strong> ${nc.isClosed ? 'Completed' : 'Open'}</p>
                                                            <p><strong>Job No:</strong> ${nc.jobNumber}</p>
                                                            <p><strong>Date:</strong> ${new Date(nc.date).toLocaleDateString()}</p>
                                                            <p><strong>Caused By:</strong> ${nc.causedProcess}</p>
                                                            <p><strong>Caught In:</strong> ${nc.caughtProcess}</p>
                                                        </div>
                                                        <p><strong>Issue:</strong> ${nc.details}</p>
                                                        <p><strong>Action:</strong> ${nc.action}</p>
                                                        ${nc.otherActionNotes ? `<p><strong>Other Notes:</strong> ${nc.otherActionNotes}</p>` : ''}
                                                        ${nc.isClosed ? `
                                                            <p><strong>Completion Date:</strong> ${nc.completionDate ? new Date(nc.completionDate).toLocaleDateString() : 'N/A'}</p>
                                                            <p><strong>Completed By:</strong> ${nc.completedBy || 'N/A'}</p>
                                                            <p><strong>Completion Action:</strong> ${nc.completionAction || 'N/A'}</p>
                                                        ` : ''}
                                                        <div class="nc-item-actions">
                                                            <button class="edit-btn" data-nc-id="${nc.id}" data-job-id="${nc.jobId}"><i class="fas fa-edit"></i> Edit</button>
                                                        </div>
                                                    `;
                                li.querySelector('.edit-btn').addEventListener('click', (e) => {
                                    const jobId = parseInt(e.target.dataset.jobId);
                                    const ncId = parseInt(e.target.dataset.ncId);
                                    const job = jobs.find(j => j.id === jobId);
                                    const nc = job ? job.nonConformances.find(n => n.id === ncId) : null;
                                    if (job && nc) {
                                        closeManageAllNcModal(); // Close this modal first
                                        openSubmitNcModal(job, nc);
                                    }
                                });
                                allNcList.appendChild(li);
                            });
                        } else {
                            allNcList.innerHTML = '<p class="placeholder-text">No non-conformances recorded across all jobs matching filters.</p>';
                        }

                        manageAllNcModal.classList.add('visible');
                    };

                    const clearNcFilters = () => {
                        ncSearchInput.value = '';
                        ncFilterCaughtInSelect.value = '';
                        ncFilterCausedBySelect.value = '';
                        ncFilterStatusSelect.value = 'open'; // Reset to default 'open'
                        openManageAllNcModal(); // Re-render with no filters
                    };

                    const closeManageAllNcModal = () => {
                        manageAllNcModal.classList.remove('visible');
                        // Do NOT clear filters here, let clearNcFilters handle it if needed
                    };

                    const downloadNcList = () => {
                        let allNonConformances = [];
                        jobs.forEach(job => {
                            if (job.nonConformances && job.nonConformances.length > 0) {
                                job.nonConformances.forEach(nc => {
                                    allNonConformances.push({
                                        ncId: nc.id,
                                        jobDescription: job.description,
                                        jobNumber: job.jobNumber,
                                        status: nc.isClosed ? 'Completed' : 'Open',
                                        caughtProcess: nc.caughtProcess,
                                        causedProcess: nc.causedProcess,
                                        date: new Date(nc.date).toLocaleDateString(),
                                        details: nc.details.replace(/"/g, '""'), // Escape double quotes for CSV
                                        action: nc.action.replace(/"/g, '""'),
                                        otherActionNotes: (nc.otherActionNotes || '').replace(/"/g, '""'),
                                        completionDate: nc.completionDate ? new Date(nc.completionDate).toLocaleDateString() : '',
                                        completedBy: (nc.completedBy || '').replace(/"/g, '""'),
                                        completionAction: (nc.completionAction || '').replace(/"/g, '""')
                                    });
                                });
                            }
                        });

                        if (allNonConformances.length === 0) {
                            showMessage('No NCs to download.', 'warning');
                            return;
                        }

                        const headers = ["NC ID", "Job Description", "Job Number", "Status", "Caught Process", "Caused Process", "Date", "Details of Issue", "Requested Action", "Other Action Notes", "Completion Date", "Completed By", "Completion Action"];
                        let csvContent = headers.join(',') + '\n';

                        allNonConformances.forEach(nc => {
                            const row = [
                                nc.ncId,
                                `"${nc.jobDescription}"`,
                                `"${nc.jobNumber}"`,
                                `"${nc.status}"`,
                                `"${nc.caughtProcess}"`,
                                `"${nc.causedProcess}"`,
                                `"${nc.date}"`,
                                `"${nc.details}"`,
                                `"${nc.action}"`,
                                `"${nc.otherActionNotes}"`,
                                `"${nc.completionDate}"`,
                                `"${nc.completedBy}"`,
                                `"${nc.completionAction}"`
                            ];
                            csvContent += row.join(',') + '\n';
                        });

                        const blob = new Blob([csvContent], { type: 'text/csv;charset=utf-8;' });
                        const link = document.createElement('a');
                        link.href = URL.createObjectURL(blob);
                        link.setAttribute('download', 'non_conformances.csv');
                        document.body.appendChild(link);
                        link.click();
                        document.body.removeChild(link);
                        showMessage('NC list downloaded successfully!', 'success');
                    };


                    // --- Process Management Functions ---

                    const renderProcessList = () => {
                        manageProcessesList.innerHTML = '';
                        if (processOptions.length === 0) {
                            manageProcessesList.innerHTML = '<p class="placeholder-text">No processes defined yet. Add one above!</p>';
                            return;
                        }

                        processOptions.forEach(process => {
                            const li = document.createElement('li');
                            li.classList.add('process-item');
                            li.dataset.name = process.name;

                            li.innerHTML = `
                                                    <div class="process-item-header">
                                                        <span>${process.name}</span>
                                                        <div class="process-item-actions">
                                                            <button class="edit-btn" data-process-name="${process.name}"><i class="fas fa-edit"></i> Edit</button>
                                                            <button class="delete-btn" data-process-name="${process.name}"><i class="fas fa-trash-alt"></i> Delete</button>
                                                        </div>
                                                    </div>
                                                    <div class="process-item-details">
                                                        <span>Default Assigned To: ${process.defaultAssignedTo || 'N/A'}</span>
                                                        <span>Outside Process: ${process.defaultIsOutsideProcess ? `Yes (${process.defaultOutsideProvider || 'N/A'})` : 'No'}</span>
                                                    </div>
                                                `;

                            li.querySelector('.edit-btn').addEventListener('click', () => openAddEditProcessModal(process));
                            li.querySelector('.delete-btn').addEventListener('click', () => deleteProcess(process.name));
                            manageProcessesList.appendChild(li);
                        });
                        saveData();
                    };

                    const openAddEditProcessModal = (process = null) => {
                        currentProcessToEdit = process;
                        if (process) {
                            addEditProcessModalTitle.textContent = `Edit Process: ${process.name}`;
                            processNameInput.value = process.name;
                            processDefaultAssignedToInput.value = process.defaultAssignedTo || '';
                            processDefaultIsOutsideProcessSelect.value = process.defaultIsOutsideProcess ? 'true' : 'false';
                            processDefaultOutsideProviderInput.value = process.defaultOutsideProvider || '';
                        } else {
                            addEditProcessModalTitle.textContent = 'Add New Process';
                            processNameInput.value = '';
                            processDefaultAssignedToInput.value = '';
                            processDefaultIsOutsideProcessSelect.value = 'false';
                            processDefaultOutsideProviderInput.value = '';
                        }

                        if (processDefaultIsOutsideProcessSelect.value === 'true') {
                            processDefaultOutsideProviderRow.classList.remove('hidden-input');
                        } else {
                            processDefaultOutsideProviderRow.classList.add('hidden-input');
                            processDefaultOutsideProviderInput.value = '';
                        }

                        clearValidationHighlights(addEditProcessModal); // Clear highlights on open
                        addEditProcessModal.classList.add('visible');
                    };

                    const closeAddEditProcessModal = () => {
                        addEditProcessModal.classList.remove('visible');
                        currentProcessToEdit = null;
                        clearValidationHighlights(addEditProcessModal); // Clear highlights on close
                    };

                    const saveProcessDetails = () => {
                        const name = processNameInput.value.trim();
                        const defaultAssignedTo = processDefaultAssignedToInput.value.trim();
                        const defaultIsOutsideProcess = processDefaultIsOutsideProcessSelect.value === 'true';
                        const defaultOutsideProvider = defaultIsOutsideProcess ? processDefaultOutsideProviderInput.value.trim() : '';

                        // Validate required fields
                        const requiredInputs = [processNameInput];
                        if (!validateAndHighlight(requiredInputs, addEditProcessModalValidationMessage)) {
                            return; // Stop if validation fails
                        }

                        if (currentProcessToEdit) {
                            const existingProcessIndex = processOptions.findIndex(p => p.name === currentProcessToEdit.name);
                            if (existingProcessIndex !== -1) {
                                if (name !== currentProcessToEdit.name && processOptions.some(p => p.name === name)) {
                                    showModalValidationMessage(addEditProcessModalValidationMessage, 'Process with this name already exists.');
                                    processNameInput.classList.add('input-error');
                                    return;
                                }
                                if (name !== currentProcessToEdit.name) {
                                    jobs.forEach(job => {
                                        job.requiredProcesses = job.requiredProcesses.map(pName => pName === currentProcessToEdit.name ? name : pName);
                                        // Update NCs if process name changes
                                        job.nonConformances.forEach(nc => {
                                            if (nc.caughtProcess === currentProcessToEdit.name) nc.caughtProcess = name;
                                            if (nc.causedProcess === currentProcessToEdit.name) nc.causedProcess = name;
                                        });
                                    });
                                }

                                processOptions[existingProcessIndex] = {
                                    name,
                                    defaultAssignedTo,
                                    defaultIsOutsideProcess,
                                    defaultOutsideProvider
                                };
                                showMessage(`Process "${name}" updated successfully.`, 'success');
                            }
                        } else {
                            if (processOptions.some(p => p.name === name)) {
                                showModalValidationMessage(addEditProcessModalValidationMessage, 'Process with this name already exists.');
                                processNameInput.classList.add('input-error');
                                return;
                            }
                            processOptions.push({
                                name,
                                defaultAssignedTo,
                                defaultIsOutsideProcess,
                                defaultOutsideProvider
                            });
                            showMessage(`Process "${name}" added successfully.`, 'success');
                        }

                        processOptions.sort((a, b) => a.name.localeCompare(b.name));

                        populateProcessDropdowns();
                        populateNcFilterDropdowns(); // Re-populate NC filter dropdowns
                        renderProcessList();
                        renderJobs();
                        closeAddEditProcessModal();
                    };

                    const deleteProcess = (processName) => {
                        if (window.confirm(`Are you sure you want to delete the process "${processName}"? Jobs currently using this process in their workflow will have it removed.`)) {
                            processOptions = processOptions.filter(p => p.name !== processName);

                            jobs.forEach(job => {
                                const originalLength = job.requiredProcesses.length;
                                job.requiredProcesses = job.requiredProcesses.filter(pName => pName !== processName);

                                // Remove NCs associated with the deleted process
                                job.nonConformances = job.nonConformances.filter(nc =>
                                    nc.caughtProcess !== processName && nc.causedProcess !== processName
                                );

                                if (job.currentProcessStep >= job.requiredProcesses.length && job.requiredProcesses.length > 0) {
                                    job.currentProcessStep = job.requiredProcesses.length - 1;
                                } else if (job.requiredProcesses.length === 0) {
                                    job.currentProcessStep = 0;
                                    job.completed = true;
                                    job.completionDate = new Date().toISOString().slice(0, 10);
                                }
                            });

                            if (currentProcessView === processName) {
                                currentProcessView = processOptions.length > 0 ? processOptions[0].name : '';
                                processSelectNav.value = currentProcessView;
                            }

                            showMessage(`Process "${processName}" deleted.`, 'success');
                            populateProcessDropdowns();
                            populateNcFilterDropdowns(); // Re-populate NC filter dropdowns
                            renderProcessList();
                            renderJobs();
                        }
                    };

                    const viewJobDrawing = (jobId) => {
                        const job = jobs.find(j => j.id === jobId);
                        if (!job) {
                            showMessage('Job not found.', 'error');
                            return;
                        }

                        if (job.drawingPdfBase64) {
                            window.open(job.drawingPdfBase64, '_blank');
                        } else if (job.eDrawingReference) {
                            // For eDrawings or other local files, we can't directly open them in a browser tab
                            // unless they are web-accessible URLs. For local paths, this would typically
                            // involve a server-side handler or a specific desktop application.
                            // For this web app, we'll just show a message.
                            showMessage(`Please open the eDrawing/reference file manually: ${job.eDrawingReference}`, 'info');
                        } else if (job.drawingNumber) {
                            // Assuming a convention like 'drawings/D-98765.pdf'
                            const pdfPath = `drawings/${job.drawingNumber}.pdf`;
                            window.open(pdfPath, '_blank');
                        } else {
                            showMessage('No drawing or PDF specified for this job.', 'error');
                        }
                    };


                    // --- View Toggling ---

                    const showMainView = (view) => {
                        currentMainView = view;

                        showActiveJobsBtn.classList.remove('active');
                        showCompletedJobsBtn.classList.remove('active');
                        showAllJobsBtn.classList.remove('active');
                        showManageProcessesBtn.classList.remove('active');

                        activeDashboard.classList.add('hidden');
                        completedJobsSection.classList.add('hidden');
                        allJobsMasterListSection.classList.add('hidden');
                        manageProcessesSection.classList.add('hidden');

                        if (view === 'active') {
                            activeDashboard.classList.remove('hidden');
                            showActiveJobsBtn.classList.add('active');
                            if (processOptions.length > 0 && !processOptions.some(p => p.name === currentProcessView)) {
                                currentProcessView = processOptions[0].name;
                            } else if (processOptions.length === 0) {
                                currentProcessView = '';
                            }
                            processSelectNav.value = currentProcessView;
                            processSelectNav.style.display = 'inline-block';
                        } else {
                            processSelectNav.style.display = 'none';
                            if (view === 'completed') {
                                completedJobsSection.classList.remove('hidden');
                                showCompletedJobsBtn.classList.add('active');
                            } else if (view === 'allJobs') {
                                allJobsMasterListSection.classList.remove('hidden');
                                showAllJobsBtn.classList.add('active');
                            } else if (view === 'manageProcesses') {
                                manageProcessesSection.classList.remove('hidden');
                                showManageProcessesBtn.classList.add('active');
                                renderProcessList();
                            }
                        }
                        renderJobs();
                    };

                    const showProcessQueue = (processName) => {
                        currentProcessView = processName;
                        renderJobs();
                    };

                    // --- Printing Functions ---

                    const generatePrintableContent = (jobsToPrint, title) => {
                        let contentHtml = `
                                                <!DOCTYPE html>
                                                <html lang="en">
                                                <head>
                                                    <meta charset="UTF-8">
                                                    <meta name="viewport" content="width=device-width, initial-scale=1.0">
                                                    <title>${title}</title>
                                                    <style>
                                                        body { font-family: 'Inter', sans-serif; margin: 20px; color: #333; }
                                                        h1 { text-align: center; margin-bottom: 30px; font-size: 2em; color: #000; }
                                                        .job-item-print {
                                                            border: 1px solid #eee;
                                                            padding: 15px;
                                                            margin-bottom: 15px;
                                                            border-radius: 8px;
                                                            background-color: #f9f9f9;
                                                            page-break-inside: avoid;
                                                        }
                                                        .job-item-print h3 {
                                                            margin-top: 0;
                                                            margin-bottom: 10px;
                                                            color: #0056b3;
                                                            font-size: 1.2em;
                                                        }
                                                        .job-item-print p {
                                                            margin: 5px 0;
                                                            font-size: 0.9em;
                                                        }
                                                        .job-item-print p strong {
                                                            color: #343a40;
                                                        }
                                                        .job-item-print .notes-print {
                                                            margin-top: 10px;
                                                            padding-top: 10px;
                                                            border-top: 1px dashed #ddd;
                                                            font-style: italic;
                                                            color: #6c757d;
                                                        }
                                                        .job-item-print.completed-print {
                                                            background-color: #e9f5e9;
                                                            opacity: 1;
                                                        }
                                                        .job-item-print.late-print {
                                                            background-color: #f2dede;
                                                        }
                                                        .non-conformance-print-section {
                                                            margin-top: 10px;
                                                            padding-top: 10px;
                                                            border-top: 1px dashed #ddd;
                                                        }
                                                        .non-conformance-print-section h4 {
                                                            font-size: 1em;
                                                            margin-bottom: 5px;
                                                            color: #0056b3;
                                                        }
                                                        .non-conformance-print-section .nc-item-print {
                                                            background-color: #e6f7ff;
                                                            border: 1px solid #b3e0ff;
                                                            border-radius: 5px;
                                                            padding: 8px;
                                                            margin-bottom: 8px;
                                                        }
                                                        .non-conformance-print-section .nc-item-print p {
                                                            margin: 3px 0;
                                                        }
                                                        .nc-item-print-grid { /* New style for NC print grid */
                                                            display: grid;
                                                            grid-template-columns: repeat(auto-fit, minmax(120px, 1fr));
                                                            gap: 5px;
                                                        }
                                                        /* Removed general-notes-print styles */
                                                    </style>
                                                </head>
                                                <body>
                                                    <h1 class="print-page-title">${title}</h1>
                                            `;

                        contentHtml += `<div class="print-job-list">`;

                        if (jobsToPrint.length === 0) {
                            contentHtml += '<p style="text-align: center; color: #666;">No jobs to display in this list.</p>';
                        } else {
                            jobsToPrint.forEach(job => {
                                const currentProcessName = job.requiredProcesses[job.currentProcessStep];
                                const currentProcessObj = processOptions.find(p => p.name === currentProcessName);
                                const assignedTo = currentProcessObj ? (currentProcessObj.defaultAssignedTo || 'N/A') : 'N/A';
                                const isOutsideProcess = currentProcessObj ? currentProcessObj.defaultIsOutsideProcess : false;
                                const outsideProvider = currentProcessObj ? (currentProcessObj.defaultOutsideProvider || 'N/A') : 'N/A';
                                const dueDate = job.dueDate ? new Date(job.dueDate).toLocaleDateString() : 'N/A';
                                const addedDate = new Date(job.addedDate).toLocaleDateString();
                                const completionDate = job.completed ? new Date(job.completionDate).toLocaleDateString() : 'N/A';
                                const pdfDisplayText = job.drawingPdfBase64
                                    ? (job.drawingPdfFileName ? `PDF Drawing: ${job.drawingPdfFileName}` : 'PDF Drawing: Attached')
                                    : 'N/A';

                                let jobClass = '';
                                if (job.completed) jobClass += ' completed-print';
                                if (new Date(job.dueDate) < new Date() && !job.completed) jobClass += ' late-print';


                                contentHtml += `
                                                        <div class="job-item-print${jobClass}">
                                                            <h3>${job.description} ${job.isRework ? '(REWORK)' : ''}</h3>
                                                            <p><strong>Job Number:</strong> ${job.jobNumber}</p>
                                                            <p><strong>Customer:</strong> ${job.customer}</p>
                                                            ${job.drawingNumber ? `<p><strong>Drawing No:</strong> ${job.drawingNumber}</p>` : ''}
                                                            ${pdfDisplayText !== 'N/A' ? `<p><strong>${pdfDisplayText.split(':')[0]}:</strong> ${pdfDisplayText.split(':')[1] || 'Attached'}</p>` : ''}
                                                            <p><strong>Quantity:</strong> ${job.quantity || 1}</p>
                                                            ${job.reworkQuantity !== undefined && job.reworkQuantity !== null && job.reworkQuantity > 0 ? `<p><strong>Rework Quantity:</strong> ${job.reworkQuantity}</p>` : ''}
                                                            <p><strong>Due Date:</strong> ${dueDate}</p>
                                                            <p><strong>Priority:</strong> ${job.priority.charAt(0).toUpperCase() + job.priority.slice(1)}</p>
                                                            <p><strong>ASME Job:</strong> ${job.isASME ? 'True' : 'False'}</p>
                                                            <p><strong>Current Process:</strong> ${currentProcessName || 'Not Set'} (${job.currentProcessStep + 1} of ${job.requiredProcesses.length})</p>
                                                            <p><strong>Assigned To:</strong> ${assignedTo}</p>
                                                            <p><strong>Outside Process:</strong> ${isOutsideProcess ? `Yes (${outsideProvider})` : 'No'}</p>
                                                            <p><strong>Workflow:</strong> ${job.requiredProcesses.join(' &#8594; ') || 'Not Defined'}</p>
                                                            <p><strong>Added On:</strong> ${addedDate}</p>
                                                            ${job.completed ? `<p><strong>Completed On:</strong> ${completionDate}</p>` : ''}
                                                            ${job.reworkCost !== undefined && job.reworkCost !== null && job.reworkCost > 0 ? `<p><strong>Rework Cost:</strong> $${job.reworkCost.toFixed(2)}</p>` : ''}
                                                            ${job.scrapCost !== undefined && job.scrapCost !== null && job.scrapCost > 0 ? `<p><strong>Scrap Cost:</strong> $${job.scrapCost.toFixed(2)}</p>` : ''}
                                                            ${job.jobFilesFolderLocation ? `<p><strong>Job Files Folder:</strong> ${job.jobFilesFolderLocation}</p>` : ''}
                                                            ${job.dxfFilesFolderLocation ? `<p><strong>DXF Files Folder:</strong> ${job.dxfFilesFolderLocation}</p>` : ''}
                                                            ${job.notes ? `<p class="notes-print"><strong>Notes:</strong> ${job.notes}</p>` : ''}
                                                            ${job.shopNotes ? `<p class="notes-print"><strong>Shop Notes:</strong> ${job.shopNotes}</p>` : ''}
                                                            ${job.nonConformances && job.nonConformances.length > 0 ? `
                                                                <div class="non-conformance-print-section">
                                                                    <h4>Non-Conformances:</h4>
                                                                    ${job.nonConformances.map(nc => `
                                                                        <div class="nc-item-print">
                                                                            <div class="nc-item-print-grid">
                                                                                <p><strong>NC ID:</strong> ${nc.id}</p>
                                                                                <p><strong>Status:</strong> ${nc.isClosed ? 'Completed' : 'Open'}</p>
                                                                                <p><strong>Caught In:</strong> ${nc.caughtProcess}</p>
                                                                                <p><strong>Caused By:</strong> ${nc.causedProcess}</p>
                                                                                <p><strong>Date:</strong> ${new Date(nc.date).toLocaleDateString()}</p>
                                                                                <p><strong>Action:</strong> ${nc.action}</p>
                                                                            </div>
                                                                            <p><strong>Details:</strong> ${nc.details}</p>
                                                                            ${nc.otherActionNotes ? `<p><strong>Other Action Notes:</strong> ${nc.otherActionNotes}</p>` : ''}
                                                                            ${nc.isClosed ? `
                                                                                <p><strong>Completion Date:</strong> ${new Date(nc.completionDate).toLocaleDateString()}</p>
                                                                                <p><strong>Completed By:</strong> ${nc.completedBy}</p>
                                                                                <p><strong>Completion Action:</strong> ${nc.completionAction}</p>
                                                                            ` : ''}
                                                                        </div>
                                                                    `).join('')}
                                                                </div>
                                                            ` : ''}
                                                        </div>
                                                    `;
                            });
                        }

                        contentHtml += `
                                                    </div>
                                                </body>
                                                </html>
                                            `;
                        return contentHtml;
                    };

                    const printWindow = (content, title) => {
                        const printWin = window.open('', '_blank', 'width=800,height=600');
                        printWin.document.write(content);
                        printWin.document.close();
                        printWin.focus();
                        printWin.print();
                    };

                    const printCurrentProcessJobs = () => {
                        const jobsToPrint = jobs.filter(job => !job.completed && job.requiredProcesses[job.currentProcessStep] === currentProcessView);
                        const title = `Jobs in ${currentProcessView} Queue`;
                        const content = generatePrintableContent(jobsToPrint, title);
                        printWindow(content, title);
                    };

                    const printAllJobs = () => {
                        const title = 'All Jobs Master List';
                        const content = generatePrintableContent(jobs, title);
                        printWindow(content, title);
                    };

                    const printShippingList = () => {
                        const shippingJobs = jobs.filter(job => !job.completed && job.requiredProcesses[job.currentProcessStep] === 'Shipping');
                        const title = 'Shipping List';
                        const content = generatePrintableContent(shippingJobs, title);
                        printWindow(content, title);
                    };

                    // --- Alert Management Modal Functions ---
                    const openAddEditAlertModal = () => {
                        newAlertMessageInput.value = '';
                        currentAlertIndex = null; // Reset current alert index
                        addUpdateAlertBtn.textContent = 'Add Alert';
                        addUpdateAlertBtn.classList.remove('save-btn');
                        addUpdateAlertBtn.classList.add('add-btn-modal');
                        addEditAlertModalTitle.textContent = 'Manage System Alerts'; // Reset title
                        renderAlertList(); // Render the list of alerts
                        clearValidationHighlights(addEditAlertModal); // Clear highlights on open
                        addEditAlertModal.classList.add('visible');
                    };

                    const renderAlertList = () => {
                        activeAlertsList.innerHTML = '';
                        if (storedAlerts.length === 0) {
                            activeAlertsList.innerHTML = '<p class="placeholder-text">No active alerts. Add one above!</p>';
                            return;
                        }

                        storedAlerts.forEach((alert, index) => {
                            const li = document.createElement('li');
                            li.classList.add('alert-item');
                            li.innerHTML = `
                                                    <span class="alert-item-message">${alert.message}</span>
                                                    <div class="alert-item-actions">
                                                        <button class="edit-alert-btn" data-index="${index}"><i class="fas fa-edit"></i> Edit</button>
                                                        <button class="delete-alert-btn" data-index="${index}"><i class="fas fa-trash-alt"></i> Delete</button>
                                                    </div>
                                                `;
                            li.querySelector('.edit-alert-btn').addEventListener('click', () => editAlert(index));
                            li.querySelector('.delete-alert-btn').addEventListener('click', () => deleteAlert(index));
                            activeAlertsList.appendChild(li);
                        });
                    };

                    const addUpdateAlert = () => {
                        const message = newAlertMessageInput.value.trim();
                        const type = 'warning'; // Always set to warning

                        // Validate required fields
                        const requiredInputs = [newAlertMessageInput];
                        if (!validateAndHighlight(requiredInputs, addEditAlertModalValidationMessage)) {
                            return; // Stop if validation fails
                        }

                        if (message) {
                            if (currentAlertIndex !== null) {
                                // Update existing alert
                                storedAlerts[currentAlertIndex].message = message;
                                showMessage('Alert updated!', 'success');
                            } else {
                                // Add new alert
                                storedAlerts.push({ message, type });
                                showMessage('Alert added!', 'success');
                            }
                            newAlertMessageInput.value = '';
                            currentAlertIndex = null; // Reset after adding/updating
                            addUpdateAlertBtn.textContent = 'Add Alert'; // Reset button text
                            addUpdateAlertBtn.classList.remove('save-btn');
                            addUpdateAlertBtn.classList.add('add-btn-modal');
                            renderAlertBanners(); // Update the main banner
                            renderAlertList(); // Update the list in the modal
                            saveData(); // Save alerts to localStorage
                        } else {
                            showMessage('Please enter an alert message.', 'error');
                        }
                    };

                    const editAlert = (index) => {
                        currentAlertIndex = index;
                        newAlertMessageInput.value = storedAlerts[index].message;
                        addUpdateAlertBtn.textContent = 'Save Changes';
                        addUpdateAlertBtn.classList.remove('add-btn-modal');
                        addUpdateAlertBtn.classList.add('save-btn');
                        addEditAlertModalTitle.textContent = 'Edit System Alert';
                        clearValidationHighlights(addEditAlertModal); // Clear highlights when editing
                    };

                    const deleteAlert = (index) => {
                        if (window.confirm('Are you sure you want to delete this alert?')) {
                            storedAlerts.splice(index, 1);
                            showMessage('Alert deleted!', 'info');
                            renderAlertBanners(); // Update the main banner
                            renderAlertList(); // Update the list in the modal
                            // If the deleted alert was being edited, clear the input
                            if (currentAlertIndex === index) {
                                newAlertMessageInput.value = '';
                                currentAlertIndex = null;
                                addUpdateAlertBtn.textContent = 'Add Alert';
                                addUpdateAlertBtn.classList.remove('save-btn');
                                addUpdateAlertBtn.classList.add('add-btn-modal');
                                addEditAlertModalTitle.textContent = 'Manage System Alerts';
                            } else if (currentAlertIndex > index) {
                                // Adjust currentAlertIndex if an earlier alert was deleted
                                currentAlertIndex--;
                            }
                            saveData(); // Save alerts to localStorage
                        }
                    };

                    const clearAllAlerts = () => {
                        if (window.confirm('Are you sure you want to clear ALL active alerts?')) {
                            storedAlerts = [];
                            renderAlertBanners();
                            renderAlertList(); // Clear the list in the modal
                            newAlertMessageInput.value = ''; // Clear input field
                            currentAlertIndex = null; // Reset editing state
                            addUpdateAlertBtn.textContent = 'Add Alert'; // Reset button text
                            addUpdateAlertBtn.classList.remove('save-btn');
                            addUpdateAlertBtn.classList.add('add-btn-modal');
                            addEditAlertModalTitle.textContent = 'Manage System Alerts';
                            showMessage('All alerts cleared.', 'info');
                            saveData(); // Save alerts to localStorage
                        }
                    };

                    const closeAddEditAlertModal = () => {
                        addEditAlertModal.classList.remove('visible');
                        newAlertMessageInput.value = '';
                        currentAlertIndex = null;
                        addUpdateAlertBtn.textContent = 'Add Alert';
                        addUpdateAlertBtn.classList.remove('save-btn');
                        addUpdateAlertBtn.classList.add('add-btn-modal');
                        addEditAlertModalTitle.textContent = 'Manage System Alerts';
                        clearValidationHighlights(addEditAlertModal); // Clear highlights on close
                    };

                    // --- Job Notes Modal Functions ---
                    const openShopNotesModal = (job) => {
                        currentJobForNotes = job;
                        shopNotesModalTitle.textContent = `Shop Notes for Job: ${job.description}`;
                        shopNotesInput.value = job.shopNotes || '';
                        shopNotesModal.classList.add('visible');
                    };

                    const saveShopNotes = () => {
                        if (currentJobForNotes) {
                            currentJobForNotes.shopNotes = shopNotesInput.value.trim();
                            showMessage(`Shop notes for "${currentJobForNotes.description}" saved.`, 'success');
                            closeShopNotesModal();
                            renderJobs(); // Re-render to show updated notes
                        }
                    };

                    const closeShopNotesModal = () => {
                        shopNotesModal.classList.remove('visible');
                        currentJobForNotes = null;
                    };

                    // --- New Folder Path Utility ---
                    const copyToClipboardAndNotify = (text, message) => {
                        if (!text) {
                            showMessage('No folder path specified.', 'warning');
                            return;
                        }
                        try {
                            // Use document.execCommand for broader compatibility in iframes
                            const textarea = document.createElement('textarea');
                            textarea.value = text;
                            document.body.appendChild(textarea);
                            textarea.select();
                            document.execCommand('copy');
                            document.body.removeChild(textarea);
                            showMessage(message, 'success');
                        } catch (err) {
                            console.error('Failed to copy text: ', err);
                            showMessage('Failed to copy path. Please copy manually: ' + text, 'error');
                        }
                    };


                    // --- Event Listeners for conditional display ---
                    processDefaultIsOutsideProcessSelect.addEventListener('change', (e) => {
                        if (e.target.value === 'true') {
                            processDefaultOutsideProviderRow.classList.remove('hidden-input');
                        } else {
                            processDefaultOutsideProviderRow.classList.add('hidden-input');
                            processDefaultOutsideProviderInput.value = '';
                        }
                    });


                    // --- Event Listeners ---
                    showAddJobModalBtn.addEventListener('click', () => openAddJobModal()); // No prepopulation
                    modalAddJobBtn.addEventListener('click', saveJob);
                    modalCancelAddJobBtn.addEventListener('click', closeAddJobModal);

                    generateCashJobNoBtn.addEventListener('click', generateCashJobNumber);

                    // New folder open buttons in Add/Edit Job Modal
                    openJobFilesFolderBtn.addEventListener('click', () => {
                        copyToClipboardAndNotify(modalJobFilesFolderLocation.value, 'Job files folder path copied to clipboard!');
                    });
                    openDxfFilesFolderBtn.addEventListener('click', () => {
                        copyToClipboardAndNotify(modalDxfFilesFolderLocation.value, 'DXF files folder path copied to clipboard!');
                    });


                    addProcessToListBtn.addEventListener('click', addProcessToList);

                    // Add listener to requiredProcessesList to clear validation message if items are added/removed
                    requiredProcessesList.addEventListener('DOMSubtreeModified', () => {
                        if (tempRequiredProcesses.length > 0) {
                            hideModalValidationMessage(requiredProcessesValidationMessage);
                        }
                    });


                    uploadCsvBtn.addEventListener('click', handleCsvUpload);

                    showActiveJobsBtn.addEventListener('click', () => showMainView('active'));
                    showCompletedJobsBtn.addEventListener('click', () => showMainView('completed'));
                    showAllJobsBtn.addEventListener('click', () => showMainView('allJobs'));
                    showManageProcessesBtn.addEventListener('click', () => showMainView('manageProcesses'));

                    processSelectNav.addEventListener('change', (e) => showProcessQueue(e.target.value));

                    printCurrentProcessJobsBtn.addEventListener('click', printCurrentProcessJobs);
                    printAllJobsBtn.addEventListener('click', printAllJobs);
                    printShippingListBtn.addEventListener('click', printShippingList);

                    manageAlertsBtn.addEventListener('click', openAddEditAlertModal);
                    addUpdateAlertBtn.addEventListener('click', addUpdateAlert); // Updated listener
                    clearAllAlertsBtn.addEventListener('click', clearAllAlerts);
                    cancelAlertModalBtn.addEventListener('click', closeAddEditAlertModal);

                    // Job-specific Shop Notes Modal Listeners
                    saveShopNotesBtn.addEventListener('click', saveShopNotes);
                    cancelShopNotesBtn.addEventListener('click', closeShopNotesModal);

                    // Edit Status Modal (Manage Job Workflow)
                    saveCostsBtn.addEventListener('click', saveJobCosts); // New listener for saving costs
                    prevStageBtn.addEventListener('click', moveToPreviousStage);
                    nextStageBtn.addEventListener('click', moveToNextStage);
                    toggleCompleteBtn.addEventListener('click', toggleJobCompletionFromModal);
                    cancelEditStatusBtn.addEventListener('click', closeEditStatusModal);
                    submitNcBtn.addEventListener('click', () => openSubmitNcModal(currentJobToEdit)); // New listener for Submit NC

                    // Non-Conformance (NC) Modal Listeners
                    submitNcDetailsBtn.addEventListener('click', submitNonConformance);
                    // Removed completeNcBtn.addEventListener('click', completeNonConformance);
                    createReworkJobBtn.addEventListener('click', createReworkJob); // New listener for rework job
                    cancelNcDetailsBtn.addEventListener('click', closeSubmitNcModal);

                    // View NC Details Modal Listener
                    closeNcDetailsModalBtn.addEventListener('click', closeViewNcDetailsModal);

                    // Manage All NCs Modal Listeners
                    manageNcBtn.addEventListener('click', () => openManageAllNcModal('', '', '', 'open')); // Default to show open NCs
                    ncSearchBtn.addEventListener('click', () => openManageAllNcModal(ncSearchInput.value, ncFilterCaughtInSelect.value, ncFilterCausedBySelect.value, ncFilterStatusSelect.value)); // Pass filters
                    ncSearchInput.addEventListener('keypress', (e) => {
                        if (e.key === 'Enter') {
                            openManageAllNcModal(ncSearchInput.value, ncFilterCaughtInSelect.value, ncFilterCausedBySelect.value, ncFilterStatusSelect.value); // Pass filters
                        }
                    });
                    ncFilterCaughtInSelect.addEventListener('change', () => openManageAllNcModal(ncSearchInput.value, ncFilterCaughtInSelect.value, ncFilterCausedBySelect.value, ncFilterStatusSelect.value));
                    ncFilterCausedBySelect.addEventListener('change', () => openManageAllNcModal(ncSearchInput.value, ncFilterCaughtInSelect.value, ncFilterCausedBySelect.value, ncFilterStatusSelect.value));
                    ncFilterStatusSelect.addEventListener('change', () => openManageAllNcModal(
                        ncSearchInput.value,
                        ncFilterCaughtInSelect.value,
                        ncFilterCausedBySelect.value,
                        ncFilterStatusSelect.value // Pass the selected status filter
                    ));
                    clearNcFiltersBtn.addEventListener('click', clearNcFilters); // New listener for clear filters
                    downloadNcListBtn.addEventListener('click', downloadNcList);
                    closeManageAllNcModalBtn.addEventListener('click', closeManageAllNcModal);

                    


                    // New Process Management Modal Listeners
                    showAddProcessModalBtn.addEventListener('click', () => openAddEditProcessModal());
                    saveProcessDetailsBtn.addEventListener('click', saveProcessDetails);
                    cancelProcessDetailsBtn.addEventListener('click', closeAddEditProcessModal);

                    // Add input error clearing listeners to all modals
                    addInputErrorClearListeners(addJobModal);
                    addInputErrorClearListeners(submitNcModal);
                    addInputErrorClearListeners(addEditProcessModal);
                    addInputErrorClearListeners(addEditAlertModal);

                    // Initial load of data and render default view
                    loadData();
                    showMainView('active');
                });




            </script>
</body>
</html>
